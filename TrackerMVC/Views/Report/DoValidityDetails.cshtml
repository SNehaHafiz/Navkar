@*@model IEnumerable<TrackerMVCEntities.BusinessEntities.DoValidityEntities>*@
@{
    ViewBag.Title = "Do Validity Details";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}


<div class=" content-area">
    <div class="page-header">
        <h4 class="page-title">Already Expired</h4>
       
    </div>
    <div class="row">
        <div class="col-md-12 col-lg-12">
            <div class="card">
                <div class="card-body">


                    <div class="row">
                        <div class="col-md-12 col-lg-12">

                            <div class="table-responsive">
                                <table class="table table-bordered table-hover tbl-cart text-nowrap" id="example1" style="width:100%">
                                    <thead>
                                        <tr>

                                          
                                            <th class="wd-15p sorting">SrNo</th>
                                            <th class="wd-15p sorting">Container No</th>
                                            <th class="wd-15p sorting">Size</th>
                                            <th class="wd-15p sorting">Type</th>
                                            <th class="wd-15p sorting">In Date</th>
                                            <th class="wd-15p sorting">do validity</th>
                                            <th class="wd-15p sorting">SL Name</th>
                                            <th class="wd-15p sorting">Dwell Days</th>
                                            <th class="wd-15p sorting">offloading Yard</th>
                                           
                                        </tr>
                                    </thead>
                                    <tbody style="box-sizing :border-box;">
                                        @foreach (var d in ViewBag.DoValidityExpired  as List<TrackerMVCEntities.BusinessEntities.DoValidityEntities>)
                                        {
                                            <tr>
                                                <td>@Html.DisplayFor(m => d.SrNo)</td>
                                                <td>@Html.DisplayFor(m => d.ContainerNo)</td>
                                                <td>@Html.DisplayFor(m => d.Size)</td>
                                                <td>@Html.DisplayFor(m => d.Type)</td>
                                                <td>@Html.DisplayFor(m => d.InDate)</td>
                                                <td>@Html.DisplayFor(m => d.dovalidity)</td>
                                                <td>@Html.DisplayFor(m => d.SLName)</td>
                                                <td>@Html.DisplayFor(m => d.DwellDays)</td>
                                                <td>@Html.DisplayFor(m => d.Yardoffloading)</td>


                                            </tr>


                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- table-wrapper -->
        </div>
        <!-- section-wrapper -->
    </div>
</div>



<script type="text/javascript">
    $(document).ready(function () {
        $('#example1').DataTable({
            "bLengthChange": false,
            "bInfo": false,
            "bPaginate": false,
            //"bFilter": false,
            "paging": false,
            "dom": 'Bfrtip',
            "buttons": [{
                "extend": 'excel',
                "title": 'DoValidity_Expiring_InSeven_Days',
                "filename": 'DoValidity_Expiring_InSeven_Days_List'

            }
            ]
        });

    });
</script>