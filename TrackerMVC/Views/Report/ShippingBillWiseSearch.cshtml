<script type="text/javascript">
    function validate() {
        // alert("hifg");

        var ContainerNumvervalidate = document.getElementById('TxtSBno').value;
        var ItemNumbervalidate = document.getElementById('TxtJoNo').value;


        var blResult = Boolean;
        blResult = true;
        $("#TxtJoNo").removeClass("form-control is-invalid state-invalid")
        $("#TxtJoNo").addClass("form-control is-valid state-valid")
        $("#txtContainerNo").removeClass("form-control is-invalid state-invalid")
        $("#txtContainerNo").addClass("form-control is-valid state-valid")
        // document.getElementById('ContainerNo').style.borderColor = "Gainsboro";


        if (ContainerNumvervalidate == "") {
            //  document.getElementById('ContainerNo').style.borderColor = "red"
            $("#TxtSBno").removeClass("form-control is-valid state-valid")
            $("#TxtSBno").addClass("form-control is-invalid state-invalid")
            blResult = blResult && false;
        }


        if (ItemNumbervalidate == "") {
            $("#TxtJoNo").removeClass("form-control is-valid state-valid")
            $("#TxtJoNo").addClass("form-control is-invalid state-invalid")
            blResult = blResult && false;
        }
        if (ItemNumbervalidate == "0") {
            $("#TxtJoNo").removeClass("form-control is-valid state-valid")
            $("#TxtJoNo").addClass("form-control is-invalid state-invalid")
            blResult = blResult && false;
        }

        if (blResult == false) { alert('Please fill the required fields!'); }
        return blResult;

    };

</script>