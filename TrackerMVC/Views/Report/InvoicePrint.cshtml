<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <link href="~/css/printstyle.css" rel="stylesheet" />
    <script src="~/js/qrcode.min.js"></script>
    <script src="~/js/JsBarcode.all.min.js"></script>
    <script src="https://www.easyproject.cn/easyqrcodejs/easy.qrcode.min.js?v=10" type="text/javascript" charset="utf-8"></script>

    <style>
        body {
            color: black;
        }

        * {
            box-sizing: border-box;
        }

        .clearfix::after {
            content: "";
            clear: both;
            display: table;
        }

        .box {
            float: left;
            width: 100%;
            padding: 0px;
            border-style: solid;
            border-width: 1pt;
        }

        .boxHalf {
            float: left;
            width: 50%;
            padding: 0px;
            border: 1px solid gray;
        }

        .boxHeaderL {
            /*float: left;*/
            width: 25%;
            padding: 0px;
            text-align: left;
        }

        .boxHeaderC {
            /*float: left;*/
            width: 75%;
            padding: 0px;
            text-align: center;
        }

        .boxHeaderR {
            /*float: left;*/
            width: 75%;
            padding: 0px;
            text-align: center;
        }

        .tblBox {
            border-collapse: collapse;
            width: 100%;
            padding: 0px;
            float: left;
        }

        .headingh3 {
            font-size: 7.5pt;
            font-family: 'Segoe UI';
            padding: 2px;
            text-decoration: underline;
        }

        th {
            border: 1px solid black;
            font-size: 7.5pt;
            font-family: Segoe UI;
            background-color: white;
        }

        td {
            border: 1px solid black;
            font-size: 7.5pt;
            font-family: Segoe UI;
        }

        h5, h4 {
            font-size: 7.5pt;
            font-family: Segoe UI;
        }

        h3 {
            font-size: 7.5pt;
            font-family: 'Segoe UI';
        }

        .buttomMargin {
            margin-bottom: -20px;
            padding-bottom: 0px;
        }

        .footer {
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            background-color: red;
            color: white;
            text-align: center;
        }

        #qrcode {
            width: 60px !important;
            height: 80px !important;
            margin: 2px;
            /*margin-top: 15px;*/
        }

        .boxHeaderCentral {
            /*float: left;*/
            width: 46%;
            padding: 0px;
            text-align: center;
        }

        .boxHeaderRight {
            width: 29%;
            padding: 0px;
            text-align: center;
        }

        .boxHeaderLeft {
            /*float: left;*/
            width: 25%;
            padding: 0px;
            text-align: left;
        }
    </style>
</head>
<body>
    <div style="width:100%; text-align:center; height:100%">
        <div style="width:100%;height:100%;">
            <table class="tblBox">
                <tbody>
                    <tr>
                


                        <th style="width:46%;text-align:center;background-color:white;font-family: 'Segoe UI'">
                            @*<img height="50" width="150" style="margin-left:10px;margin-top:20px;" src="~/images/NavkarLogo.jpg" />*@
                            <span style="font-size:12pt;margin-top: 0px;margin-bottom:-13px;">@ViewBag.CompanyName</span>   <br />
                            <span style="font-size:9pt;margin-top: 0px;margin-bottom:0px;font-weight:normal"> (@ViewBag.CINNo)</span> <br />
                            <span style="font-size:9pt;margin-top: 0px;margin-bottom:0px;font-weight:normal"> (@ViewBag.RegdOffice)</span> <br />
                            <span style="font-size:7pt;margin-top: 0px;margin-bottom:0px;font-weight:normal"> @ViewBag.Addressl</span> <br />
                            <span style="font-size:7pt;margin-top: 0px;margin-bottom:0px;font-weight:normal"> @ViewBag.IMP_AC_EMAILID</span> <br />

                        </th>

                        <th style="width:39%">
                            <div class="boxHeaderRight" style="height:308px;">
                                <div id="qrcode"> </div>
                            </div>
                        </th>

                    </tr>

                </tbody>
            </table>


            <table class="tblBox">
                <tbody>
                    <tr>
                        <th style="width:100%;text-align:center; background-color: #ede5e5;font-size:12pt">
                            @ViewBag.INVheader
                        </th>
                    </tr>

                </tbody>
            </table>

            <div class="printBody clearfix">
                <div class="box" style="border-style:none;">
                 

                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <td colspan="1" style="vertical-align:top;width:56%">
                                    <div class="box" style="border-style: none;">
                                        <table class="tblBox">
                                            <tbody>

                                                <tr>
                                                    <th style="width:30%;text-align:left;border-style:none">E-Invoice Details</th>

                                                </tr>
                                                <tr>
                                                    <th style="width:35%;text-align:left;border-style:none">IRN</th>
                                                    <td style="width:65%;text-align:left;border-style:none;font-weight: 700;">:<label style="visibility:hidden;font-weight:400">Sp</label>@ViewBag.Irn</td>
                                                </tr>
                                                <tr>
                                                    <th style="width:30%;text-align:left;border-style:none">Ack.No</th>
                                                    <td style="width:70%;text-align:left;border-style:none;font-weight: 700;">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.AckNo</td>
                                                </tr>
                                                <tr>
                                                    <th style="width:30%;text-align:left;border-style:none">ACK Date</th>
                                                    <td style="width:70%;text-align:left;border-style:none;font-weight: 700;">:<label style="visibility:hidden;font-weight:400">Sp</label>@ViewBag.AckDt</td>
                                                </tr>



                                            </tbody>
                                        </table>
                                    </div>
                                </td>

                                <td colspan="1" style="vertical-align:top;width:26%;text-align:center;background-color:white">
                                
                                    <input type="checkbox" name="checklist[]" id="chkDelLoc" readonly style="font-size:7pt" checked>
                                    <span>Original for recipient </span><br />
                                    <input type="checkbox" name="checklist[]" id="chkDelLoc" readonly style="font-size:7pt">
                                    <span>Duplicate for supplier</span>
                                
                                </td>

                            </tr>
                        

                        </tbody>
                    </table>
                    <br />
                </div>
            </div>
            <div class="printBody clearfix">
                <div class="box" style="border-style:none;">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <th style="width:9%;text-align:left">Invoice No</th>
                                <td style="width:40%;text-align:left">@ViewBag.InvoiceNo</td>
                                <th style="width:9%;text-align:left">Invoice Date</th>
                                <td style="width:40%;text-align:left;font-weight:700">@ViewBag.InvoiceDate</td>
                            </tr>
                            <tr>
                                <th colspan="2" style="width:20%;text-align:left">Details of Receiver/ Billed to:</th>
                                <th colspan="2" style="width:20%;text-align:left">Details of Consignee/ Shipped to:</th>
                            </tr>
                            <tr>
                                <td colspan="2" style="vertical-align:top;width:50%">
                                    <div class="box" style="border-style: none;">
                                        <table class="tblBox">
                                            <tbody>
                                                <tr>
                                                    <th style="width:30%;text-align:left;border-style:none">Name</th>
                                                    <td style="width:70%;text-align:left;border-style:none;font-weight: 700;">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.Name</td>
                                                </tr>
                                                <tr>
                                                    <th style="width:30%;text-align:left;border-style:none">Address</th>
                                                    <td style="width:70%;text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.GSTAddress</td>
                                                </tr>
                                                <tr>
                                                    <th style="width:30%;text-align:left;border-style:none">GSTIN</th>
                                                    <td style="width:70%;text-align:left;border-style:none;font-weight: 700;">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.GSTIN</td>
                                                </tr>
                                                <tr>
                                                    <th style="width:20%;text-align:left;border-style:none">Place of Supply</th>
                                                    <td colspan="1" style="border-style:none;">
                                                        <table class="tblBox">
                                                            <tr>
                                                                <td style="width:30%;text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.PlaceofSupply</td>
                                                                <th style="width:20%;text-align:left;border-style:none;border: 1px solid gray;">State Code :</th>
                                                                <td style="width:30%;text-align:left;border-style:none;border: 1px solid gray;">@ViewBag.state_Code</td>
                                                            </tr>
                                                        </table>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </td>
                                <td colspan="2" style="vertical-align:top;width:50%">
                                    <div class="box" style="border-style: none;">
                                        <table class="tblBox">
                                            <tbody>
                                                <tr>
                                                    <th style="width:30%;text-align:left;border-style:none">Consignee</th>
                                                    <td style="width:70%;text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.Consignee</td>
                                                </tr>

                                                <tr>
                                                    <th style="width:30%;text-align:left;border-style:none">Customer</th>
                                                    <td style="width:90%;text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.AGName</td>
                                                </tr>
                                                <tr>
                                                    <th style="width:30%;text-align:left;border-style:none">Address</th>
                                                    <td style="width:90%;text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.Address</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="printBody clearfix">
                <div class="box" style="border-style: none;">
                    <table class="tblBox" style="border-style: none;">
                        <tbody>
                            <tr>
                                <td colspan="4" style="vertical-align:top">
                                    <div class="box" style="border-style: none;">
                                        <table class="tblBox">
                                            <tbody>
                                                <tr style="border-bottom: 1px solid black;">
                                                    <th colspan="2" style="width:20%;text-align:left;border-style: none;">Operational Details</th>
                                                    <th colspan="2" style="width:20%;text-align:left;border-style: none;">Movement: @ViewBag.Movement</th>
                                                </tr>
                                                <tr>
                                                    <th style="text-align:left;border-style:none;">Shipping Line</th>
                                                    <td style="text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.ShippingLine</td>
                                                    <th style="text-align:left;border-style:none">BOE No</th>
                                                    <td style="text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.BOENo</td>
                                                </tr>
                                                <tr>
                                                    <th style="width:20%;text-align:left;border-style:none">IGM/Item No</th>
                                                    <td style="width:30%;text-align:left;border-style:none;font-weight:bold">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.IgmItemNo</td>
                                                    <th style="width:20%;text-align:left;border-style:none">BL No</th>
                                                    <td style="width:30%;text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.BLNo</td>
                                                </tr>
                                                <tr>
                                                    <th style="width:20%;text-align:left;border-style:none">CHA Name</th>
                                                    <td style="width:30%;text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.CHAName</td>
                                                    <th style="width:20%;text-align:left;border-style:none">Cargo Weight</th>
                                                    <td style="width:30%;text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.CargoWeight</td>
                                                </tr>
                                                <tr>
                                                    <th style="width:20%;text-align:left;border-style:none">Cargo Desc</th>
                                                    <td colspan="3" style="width:30%;text-align:left;border-style:none">:<label style="visibility:hidden;font-weight:400">Sp</label>@ViewBag.CargoDesc</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

            <div style="text-align:left;">
                <div style="text-align:left;">
                    <h3 style="margin-top: -0px;margin-bottom: -0px;font-size: 9.5pt;">Container Details:</h3>
                </div>
            </div>
            <div class="table-responsive">
                <table class="tblBox" style="border-style: none;">
                    <thead>
                        <tr>
                            <th style="width:2px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Sr No.</th>
                            <th style="width:7px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Container No</th>
                            <th style="width:5px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Size</th>
                            <th style="width:2px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Type</th>
                            <th style="width:80px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Cargo Type</th>
                            <th style="width:50px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Scan Type</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">PKGS</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Gross Weight</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Arrival Date & Time</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Total Days</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Free Days</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Charge Days</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Hnd Charges</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Grd Chagres</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Del Mode</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Validity Date</th>
                            <th style="width:100px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">In Mode</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var dr in ViewBag.ContainerDetailsList)
                        {
                            <tr>
                                <td style="text-align:center">@dr["SRNo"]</td>
                                <td style="text-align:center">@dr["ContainerNO"]</td>
                                <td style="text-align:center">@dr["Size"]</td>
                                <td style="text-align:center">@dr["Type"]</td>
                                <td style="text-align:center">@dr["CargoType"]</td>
                                <td style="text-align:center">@dr["Scantype"]</td>
                                <td style="text-align:center">@dr["PKGS"]</td>
                                <td style="text-align:center">@dr["Wt"]</td>
                                <td style="text-align:center">@dr["Indate"]</td>
                                <td style="text-align:center">@dr["tdays"]</td>
                                <td style="text-align:center">@dr["FreeDays"]</td>
                                <td style="text-align:center">@dr["ChargeableDays"]</td>
                                <td style="text-align:center">@dr["HandlingCharges"]</td>
                                <td style="text-align:center">@dr["Groundrent"]</td>
                                <td style="text-align:center">@dr["deltype"]</td>
                                <td style="text-align:center">@dr["Validupto"]</td>
                                <td style="text-align:center">@dr["Transtype"]</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>


            <div class="table-responsive">
                <table class="tblBox" style="border-style: none;">
                    <thead>
                        <tr>
                            <th style="width:50px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Sr No.</th>
                            <th style="width:20px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Details Of Bill Item Description</th>
                            <th style="width:80px;background-color:#ede5e5;font-size: 7.5pt;font-weight: 700;">Amount</th>




                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var dr in ViewBag.HeadDetailsList)
                        {
                            <tr>
                                <td style="text-align:center">@dr["SRNo"]</td>
                                <td style="text-align:left">@dr["ChargesDesc"]</td>
                                <td style="text-align:right">@dr["Amount"]</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
            <div class="table-responsive">
                <table class="tblBox" style="border-style: none;">
                    <thead>
                        <tr>
                            <th style="width:13%;background-color: #ede5e5;font-size: 7.5pt;font-weight: 700;text-align:center" rowspan="2">Sr No.</th>
                            <th style="width:20%;background-color: #ede5e5;font-size: 7.5pt;font-weight: 700;" rowspan="2">Charges Description</th>
                            <th style="width:13%;background-color: #ede5e5;font-size: 7.5pt;font-weight: 700;" rowspan="2">Sac Code</th>
                            <th style="width:13%;background-color: #ede5e5;font-size: 7.5pt;font-weight: 700;" rowspan="2">Amount</th>
                            <th style="width:10%;background-color: #ede5e5;font-size: 7.5pt;font-weight: 700;" rowspan="2">Less Disc.</th>
                            <th style="width:15%;background-color: #ede5e5;font-size: 7.5pt;font-weight: 700;" rowspan="2">Taxable Value</th>
                            <th style="width:15%;background-color: #ede5e5;font-size: 7.5pt;font-weight: 700;" colspan="2">SGST</th>
                            <th style="width:15%;background-color: #ede5e5;font-size: 7.5pt;font-weight: 700;" colspan="2">CGST</th>
                            <th style="width:15%;background-color: #ede5e5;font-size: 7.5pt;font-weight: 700;" colspan="2">IGST</th>
                        </tr>
                        <tr>
                            <th style="width:100px;background-color: #ede5e5;">Rate(%)</th>
                            <th style="width:100px;background-color: #ede5e5;">Amount</th>
                            <th style="width:100px;background-color: #ede5e5;">Rate(%)</th>
                            <th style="width:100px;background-color: #ede5e5;">Amount</th>
                            <th style="width:100px;background-color: #ede5e5;">Rate(%)</th>
                            <th style="width:100px;background-color: #ede5e5;">Amount</th>
                        </tr>
                    </thead>
                    <tbody>

                        @foreach (var dr in ViewBag.ChargesDetailsList)
                        {
                            <tr>
                                <td style="text-align:center">@dr["SRNo"]</td>
                                <td style="text-align:center">@dr["AccountName"]</td>
                                <td style="text-align:center">@dr["HSN"]</td>
                                <td style="text-align:right">@dr["Amount"]</td>
                                <td style="text-align:center">@dr["Discount"]</td>
                                <td style="text-align:right">@dr["NetAmount"]</td>
                                <td style="text-align:center">@dr["Srate"]</td>
                                <td style="text-align:right">@dr["SGST"]</td>
                                <td style="text-align:center">@dr["CRate"]</td>
                                <td style="text-align:right">@dr["CGST"]</td>
                                <td style="text-align:center">@dr["IRate"]</td>
                                <td style="text-align:right">@dr["IGST"]</td>
                            </tr>
                        }
                    </tbody>
                    <tfoot>
                        <tr>
                            <td style="text-align:center;" colspan="2">Total</td>

                            <td style="text-align:right">@ViewBag.Amount </td>
                            <td style="text-align:center"></td>
                            <td style="text-align:right">@ViewBag.NetAmount </td>
                            <td style="text-align:center"></td>
                            <td style="text-align:right">@ViewBag.SGST </td>
                            <td style="text-align:center"> </td>
                            <td style="text-align:right">@ViewBag.CGST </td>
                            <td style="text-align:center"></td>
                            <td style="text-align:right">@ViewBag.IGST </td>
                        </tr>
                    </tfoot>
                </table>
            </div>

            <div style="border:1px solid gray;">
                <div class="printBody clearfix">
                    <div class="box" style="border-style: none;">
                        <table class="tblBox">
                            <tbody>
                                <tr>
                                    <td colspan="2" style="vertical-align:top;width:65%">
                                        <div class="box" style="border-style: none;">
                                            <table class="tblBox">
                                                <tbody>
                                                    <tr style="border-bottom: 1px solid black;">
                                                        <th style="width:20%;text-align:left;border-top-style:none;font-weight: 500;border-style: none;">Total Invoice Amount in Words :</th>
                                                        <td style="width:30%;text-align:left;font-weight: 500;border-style: none;">@ViewBag.TotalAmountInWord</td>
                                                    </tr>
                                                    <tr>
                                                        <th style="width:20%;text-align:left;font-weight: 500;border-style: none;">BANK DETAILS :</th>
                                                        <td style="width:30%;text-align:left;font-weight: 500;border-style: none;">Company Name &nbsp; @ViewBag.CompanyName</td>
                                                    </tr>
                                                    <tr>
                                                        <th style="width:20%;text-align:left;font-weight: 500;;font-weight: 500;border-style: none;">@ViewBag.BankName</th>
                                                        <td style="width:30%;text-align:left;border-style: none;">@ViewBag.AccountNo</td>
                                                    </tr>
                                                    <tr>
                                                        <th style="width:20%;text-align:left;font-weight: 500;border-style: none;">@ViewBag.BranchName</th>
                                                        <td style="width:30%;text-align:left;font-weight: 500;border-style: none;">@ViewBag.IFSCCode</td>
                                                    </tr>
                                                    <tr>
                                                        <th style="width:5%;text-align:left;font-weight: 500;border-style: none;border-top: 1px solid black;">Remarks</th>
                                                        <td style="width:30%;text-align:left;font-weight: 500;border-style: none;border-top: 1px solid black;">@ViewBag.Remarks</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </td>
                                    <td colspan="2" style="vertical-align:top;width:35%">
                                        <div class="box" style="border-style: none;">
                                            <table class="tblBox" style="border:none">
                                                <tbody>
                                                    <tr>
                                                        <th style="width:60%;text-align:left;font-weight: 500;border-bottom: 1px solid black;border-left: none !important;border-top:none !important"> Total Amount Before Tax</th>
