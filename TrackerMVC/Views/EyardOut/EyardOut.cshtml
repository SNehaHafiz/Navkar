</div>

<script>
    function BookingNo() {
        debugger;
        var BookingNo = $("#txtBookingNo").val();
        var data1 = { 'BookingNo': BookingNo };
        var data = JSON.stringify(data1);
        $.ajax({
            url: '/EyardOut/GetIGSTCGST',
            type: 'Post',
            data: data,
            contentType: 'application/json; charset=utf-8',
            success: function (data) {


                $("#txtShipperName").val(data[0].ShipperName);
                $("#txtLineName").val(data[0].LineName);
                $("#txtTransporter").val(data[0].Transporter);
                $("#ddlLocation").val(data[0].Location);

                $("#txtPOD").val(data[0].POD);
                $("#txtPorts").val(data[0].ports);
                $("#txtSealNo").val(data[0].SealNo);
                $("#ddlVessel").val(data[0].VesselID);
                $("#txtMovementType").val(data[0].MovementType);

            },
            error: function (errormessage) {
                alert(errormessage.responseText);
                $("#tracker-loader").fadeOut("slow");
            }
        });
    }


    function ContainerNo() {
        debugger;
        var ContainerNo = $("#txtContainerNo").val();
        var data1 = { 'ContainerNo': ContainerNo };
        var data = JSON.stringify(data1);
        $.ajax({
            url: '/EyardOut/GetContainer',
            type: 'Post',
            data: data,
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                debugger;
                $("#txtEntryID").val(data[0].EntryID);
                $("#txtType").val(data[0].Type);
                $("#txtSize").val(data[0].Size);
                $("#txtISOCode").val(data[0].ISOCode);
                $("#txtDamageRemarks").val(data[0].DamageRemarks);

                $("#lblTypeID").val(data[0].Size);
                $("#txtForwarderLine").val(data[0].txtForwarderLine);
                $("#lbllineid").val(data[0].lineid);
                $("#lblcusto").val(data[0].custo);
            },
            error: function (errormessage) {
                alert(errormessage.responseText);
                $("#tracker-loader").fadeOut("slow");
            }
        });
    }
    var storeAddedItem = [];
    function Add() {
        debugger;
        var element = {};
        element.ContainerNo = $('#txtContainerNo').val();
        element.Size = $('#txtSize').val();
        element.Type = $('#txtType').val();
        element.ISOCode = $('#txtISOCode').val();
        element.lineid = $('#lbllineid').val();
        element.LineName = $('#txtLineName').val();
        //$("#txtLineName").val(data[0].LineName);
        element.BookingNo = $('#txtBookingNo').val();
        element.Location = $('#ddlLocation').val();
        element.DamageRemarks = $('#txtDamageRemarks').val();
        element.VesselID = $('#ddlVessel').val();

        element.POD = $('#txtPOD').val();
        element.SealNo = $('#txtSealNo').val();
        element.EntryID = $('#TxtEntryID').val();

        element.OutStatus = $('#ddlOutStatus').val();

      storeAddedItem.push(element);

       
            $('#example').DataTable({
                "destroy": true,
                "bLengthChange": false,
                "aaData": storeAddedItem,
                "bPaginate": false,
                "bInfo": false,
                "paging": false,
                "columns": [

                    { "data": "ContainerNo" },
                    { "data": "Size" },
                    { "data": "Type" },
                    { "data": "ISOCode" },
                    { "data": "LineName" },
                    { "data": "lineid" },
                    { "data": "BookingNo" },
                    
                    { "data": "DamageRemarks" },
                    { "data": "VesselID" },
                    { "data": "POD" },
                    { "data": "SealNo" },
                    { "data": "EntryID" },
                    { "data": "OutStatus" },
                    { "data": "Location" }
                ]
            });
    }

         function Save() {
 
         debugger;
         ///ddlentryType

             var GatePassDate = $("#TxtGatePassDate").val();
             var MovementBy = $("#ddlMovementBy").val();
             var MovementType = $("#txtMovementType").val();
             var TruckNo = $("#txtTruckNo").val();
             var Transporter = $("#txtTransporter").val();
       
     
            var SelectedData = new Array();
            var tablearray = new Array();

            var table = document.getElementById("example");

            var row;
            for (var i = 1; i < table.rows.length; i++) {

                row = table.rows[i];
                ContainerNo = row.cells[0].innerText;
                Size = row.cells[1].innerText;
                Type = row.cells[16].innerText;
                ISOCode = row.cells[4].innerText;
                LineName = row.cells[5].innerText;
                lineid = row.cells[3].innerText;
                BookingNo = row.cells[6].innerText;
                VesselID = row.cells[7].innerText;
                POD = row.cells[8].innerText;
                SealNo = row.cells[9].innerText;
                EntryID = row.cells[17].innerText;
                OutStatus = row.cells[12].innerText;
                Location = row.cells[13].innerText;
                

                SelectedData.push({
                    'ContainerNo': ContainerNo, 'Size': Size, 'Type': Type, 'ISOCode': ISOCode,
                    'LineName': LineName, 'lineid': lineid, 'BookingNo': BookingNo, 'VesselID': VesselID, 'POD': POD, 'SealNo': SealNo, 'EntryID': EntryID, 'OutStatus': OutStatus,
                    'Location': Location
                })
            }

            var data1 = {
                'Debitdata': SelectedData, 'GatePassDate': GatePassDate, 'MovementBy': MovementBy, 'MovementType': MovementType, 'TruckNo': TruckNo,
                'Transporter': Transporter
            };
            var data = JSON.stringify(data1);


                          $.ajax({
                              url: "/ExpensesHead/SavePartyDebitEntryDetails",
                            data: data,
                            type: "POST",
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                              success: function (data) {
                                  alert(data);
                                  document.location = '@Url.Action("ExpensesEntryMaster", "ExpensesHead")';
                            },
                            error: function (errormessage) {
                                alert(errormessage.responseText);
                            }
                        });
 
         
    }

       
</script>