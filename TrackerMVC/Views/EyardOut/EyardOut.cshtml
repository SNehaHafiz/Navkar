                                            @Html.TextBox("txtPorts", null, new { @class = "form-control ", @id = "txtPorts", name = "txtPorts", autocomplete = "off" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-3 col-sm-6">

                            <div class="form-group">
                                <label class="form-label">Seal No</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.TextBox("txtSealNo", null, new { @class = "form-control ", @id = "txtSealNo", name = "txtSealNo", autocomplete = "off" })
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="col-lg-5 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">Location</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @*<select class="form-control" id="ddlLocation">
                                <option value="0">--Select--</option>
                                <option value="Male">Male</option>
                                <option value="Female">Female</option>
                            </select>*@
                                        @Html.DropDownList("ddlLocation", (IEnumerable<SelectListItem>)ViewBag.GroupDDL, "--Select--", new { @class = "form-control", @id = "ddlLocation", name = "ddlLocation", autocomplete = "off" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-3 col-sm-6">

                            <div class="form-group">
                                <label class="form-label">POD</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.TextBox("txtPOD", null, new { @class = "form-control ", @id = "txtPOD", name = "txtPOD", autocomplete = "off" })
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-5 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">Out Status</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        <select class="form-control" id="ddlOutStatus">
                                            <option value="0">--Select--</option>
                                            <option value="1">DM</option>
                                            <option value="2">AV</option>
                                        </select>
                                        @*@Html.DropDownList("ddlentryType", (IEnumerable<SelectListItem>)ViewBag.EntryType, "--Select--", new { @class = "form-control", @id = "ddlentryType", name = "ddlentryType", autocomplete = "off" })*@
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-5 col-sm-6">

                            <div class="form-group">
                                <label class="form-label">Damage Remarks</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.TextBox("txtDamageRemarks", null, new { @class = "form-control ", @id = "txtDamageRemarks", name = "txtDamageRemarks", autocomplete = "off" })
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="col-lg-1 col-sm-12">
                            <div class="form-group">
                                <label class="form-label" style="visibility:hidden">.</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        <button id="btnSave" class="btn btn-success btn-xs" onclick="return Add();">Add</button>
