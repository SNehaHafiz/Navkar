<script>
    function Getdetails(searchcriteria) {
        $("#divSummary").hide();
        $("#divdriverSummary").hide();
        $("#divtrailerMaasterView").hide();
        $("#DivTrailerLocation").hide();
        $("#DivgarageDetails").hide();
        $("#DivWihtourDrivers").hide();
        $("#DivAccident").hide();
        $("#Divservicicng").hide();
        $("#DivTrailerShifting").hide();
        $("#divInsuranceandRtoDetails").hide();
        $("#DivallInsuranceDetailslist").show();
        
        if (searchcriteria == "Insurance") {
            $("#getHeaderInsurance").text('Insurance Due Date');
        }

        if (searchcriteria == "National Permit Expiry") {
            $("#getHeaderInsurance").text('National Permit Expiry Due Date');
        }
        if (searchcriteria == "Local Permit Expiry") {
            $("#getHeaderInsurance").text('National Permit Expiry Due Date');
        }

        if (searchcriteria == "Tax") {
            $("#getHeaderInsurance").text('Tax Due Date');
        }

        if (searchcriteria == "Fitness") {
            $("#getHeaderInsurance").text('Fitness Due Date');
        }
        if (searchcriteria == "Green Tax") {
            $("#getHeaderInsurance").text('Green Tax Due Date');
        }

