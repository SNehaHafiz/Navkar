
@{
    ViewBag.Title = "Transporter Dashboard";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}

<style>
    .pointer_cursor {
        cursor: pointer;
    }
</style>
<script src="~/js/GetTableJsonwopaging.js"></script>

<link href="~/css/Transporter.css" rel="stylesheet" />
<div id="divSummary">
    <div class="page-header">
        <h4 class="page-title">Transporter</h4>
    </div>
    <div class="row">

        <div class="col-sm-12 col-lg-6">
            <div class="card text-center" style="border: 1px solid #9f78ff;" id="importArrivalMainCard">
                <div>
                    <div class="inline">
                        <div>
                            <img class="arrival_image" src="~/images/transporter.png">
                            <!-- <i class="mdi mdi-truck-trailer left import_icon arrival"></i> -->
                        </div>
                        <div class="widget-line mt-5 margin_wrap">
                            <p>Transporter</p>
                            <h1 class="counter">3</h1>
                        </div>

                    </div>
                </div>
                <div class="mx-auto chart-circle chart-circle-md donutShadow" data-value="1.2951" data-thickness="20" data-color="#9f78ff">
                    <div class="chart-circle-value fs"><i class="fa fa-random"></i></div>
                </div>
                <div class="widget-line mt-5 ">
                    <div class="table-responsive">
                        <table class="table card-table table-vcenter text-nowrap">
                            <thead class="text-white" style="background-color:#7bc144">
                                <tr>
                                    <th style="color:white;text-align:left;font-weight:bold;">#</th>
                                    <th style="color:white;text-align:left;font-weight:bold;">Transporter</th>
                                    <th style="color:white;text-align:left;font-weight:bold;">Code</th>
                                    <th style="color:white;text-align:left;font-weight:bold;">Drivers</th>
                                    <th style="color:white;text-align:left;font-weight:bold;">Trailers</th>


                                    @*<th style="color:white;">Total</th>*@


                                </tr>

                            </thead>
                            <tbody>
                                @foreach (var d in ViewBag.Transportername as List<TrackerMVCEntities.BusinessEntities.TransporterEntities>)
                                {

                                    <tr>
                                        <td style="text-align:right;">@d.TransID</td>
                                        <td class="bg-primary text-white" style="text-align:left;padding: 10px 10px 0px 10px; cursor: pointer;" onclick="getPersonMonthlyDetails('@d.TransName')">@d.TransName </td>
                                        <td style="text-align:right;">@d.FuelShortCode </td>
                                        <td style="text-align:right;">@d.TrailerCount </td>
                                        <td style="text-align:right;">@d.DriverCount </td>



                                    </tr>
                                }

                            </tbody>
                        </table>
                    </div>

                </div>

            </div>





        </div>
        <div class="col-sm-12 col-lg-6">

            <div class="row">
                <div class="col-lg-5 col-sm-12">
                    <div class="card bg-info " onclick="ViewSummaryDrivers()" style="cursor:pointer">
                        <div class="card-body" style="padding:0.2rem;">
                            <div class="ml-auto">
                                <span class="text-white display-6"><i class="fa fa-user-secret fa-1x"></i></span>
                            </div>
                            <div class="float-right ">
                                <div>
                                    <h5 class="text-white">Drivers</h5>
                                    <h2 class="text-white m-0 " id="PrincipalCount">@ViewBag.DriverCount</h2>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-5 col-sm-12">
                    <div class="card bg-success" onclick="ViewTrailerSummary()" style="cursor:pointer">
                        <div class="card-body" style="padding:0.2rem;">

                            <div class="ml-auto">
                                <span class="text-white display-6"><i class="fa fa-truck fa-1x"></i></span>
                            </div>
                            <div class="float-right ">
                                <div>
                                    <h5 class="text-white">Vehicles</h5>
                                    <h2 class="text-white m-0 " id="PrincipalCount">@ViewBag.TrailerCount</h2>
                                </div>

                            </div>

                        </div>
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col-lg-5 col-sm-12">
                    <div class="card bg-azure-light " onclick="ViewGargaeDetails()" style="cursor:pointer">
                        <div class="card-body" style="padding:0.2rem;">

                            <div class="ml-auto">
                                <span class="text-white display-6"><i class="fa fa-car fa-1x"></i></span>
                            </div>
                            <div class="float-right ">
                                <div>
                                    <h5 class="text-white">Garage</h5>
                                    <h2 class="text-white m-0 " id="PrincipalCount">@ViewBag.TrailergarageCount</h2>
                                </div>

                            </div>

                        </div>
                    </div>

                </div>

                <div class="col-lg-5 col-sm-12">
                    <div class="card bg-warning " onclick="ViewWithoutDrivers()" style="cursor:pointer">
                        <div class="card-body" style="padding:0.2rem;">

                            <div class="ml-auto">
                                <span class="text-white display-6"><i class="fa fa-id-card-o fa-1x"></i></span>
                            </div>
                            <div class="float-right ">
                                <div>
                                    <h5 class="text-white">Without Driver</h5>
                                    <h2 class="text-white m-0 " id="PrincipalCount">@ViewBag.DriverWihoutDrivers</h2>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col-lg-5 col-sm-12">
                    <div class="card bg-danger" onclick="ViewSummaryOFaccident()" style="cursor:pointer">
                        <div class="card-body" style="padding:0.2rem;">

                            <div class="ml-auto">
                                <span class="text-white display-6"><i class="fa fa-trash fa-1x"></i></span>
                            </div>
                            <div class="float-right ">
                                <div>
                                    <h5 class="text-white">Accidental</h5>
                                    <h2 class="text-white m-0 " id="PrincipalCount">@ViewBag.accidentDetails</h2>
                                </div>

                            </div>

                        </div>
                    </div>

                </div>
                <div class="col-lg-5 col-sm-12">
                    <div class="card bg-gray " onclick="ViewOFServivingDetails()" style="cursor:pointer">
                        <div class="card-body" style="padding:0.2rem;">

                            <div class="ml-auto">
                                <span class="text-white display-6"><i class="fa fa-chain-broken fa-1x"></i></span>
                            </div>
                            <div class="float-right ">
                                <div>
                                    <h5 class="text-white">Servicing</h5>
                                    <h2 class="text-white m-0 " id="PrincipalCount">@ViewBag.Servicing</h2>
                                </div>

                            </div>

                        </div>
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col-lg-5 col-sm-12">
                    <div class="card bg-orange" onclick="ViewSummaryTrailerShifting()" style="cursor:pointer">
                        <div class="card-body" style="padding:0.2rem;">

                            <div class="ml-auto">
                                <span class="text-white display-6"><i class="fa fa-arrows fa-1x"></i></span>
                            </div>
                            <div class="float-right ">
                                <div>
                                    <h5 class="text-white">Shifting</h5>
                                    <h2 class="text-white m-0 " id="PrincipalCount">@ViewBag.TrailerShiftingLocation</h2>
                                </div>

                            </div>

                        </div>
                    </div>

                </div>
                <div class="col-lg-5 col-sm-12">
                    <div class="card text-left bg-gradient-primary text-white" onclick="ViewInsuranceDetails()" style="cursor:pointer">
                        <div class="card-body" style="padding:0.2rem;">

                            <div class="ml-auto">
                                <span class="text-white display-6"><i class="fa fa-check-square-o fa-1x"></i></span>
                            </div>
                            <div class="float-right ">
                                <div>
                                    <h5 class="text-white">Insurance & RTO </h5>
                                    <h2 class="text-white m-0 " id="PrincipalCount">@ViewBag.InsuranceandRtoTracking</h2>
                                </div>

                            </div>

                        </div>
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col-lg-5 col-sm-12">
                    <div class="card bg-lime" onclick="WithoutPaperDetails()" style="cursor:pointer">
