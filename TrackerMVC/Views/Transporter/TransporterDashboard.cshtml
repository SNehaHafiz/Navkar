                            <div class="ml-auto">
                                <span class="text-white display-6"><i class="fa fa-files-o fa-1x"></i></span>
                            </div>
                            <div class="float-right ">
                                <div>
                                    <h5 class="text-white">Without Paper</h5>
                                    <h2 class="text-white m-0 " id="PrincipalCount">@ViewBag.WithoutPaper</h2>
                                </div>

                            </div>

                        </div>
                    </div>

                </div>
                
            </div>
        </div>


        <!--Codes By Trailers Location-->
        <div class="col-lg-6 col-sm-12">

            <div class="card text-center" style="border: 1px solid #9f78ff;" id="importArrivalMainCard">
                <div>
                    <div class="inline">
                        <div>
                            <img class="arrival_image" src="~/images/Location.png">
                            <!-- <i class="mdi mdi-truck-trailer left import_icon arrival"></i> -->
                        </div>
                        <div class="widget-line mt-5 margin_wrap">
                            <p>Various Location</p>
                            <h1 class="counter">@ViewBag.TrailerLocationcount</h1>
                        </div>

                    </div>
                </div>
                <div class="mx-auto chart-circle chart-circle-md donutShadow" data-value="1.2951" data-thickness="20" data-color="#9f78ff">
                    <div class="chart-circle-value fs"><i class="fa fa-random"></i></div>
                </div>
                <div class="widget-line mt-5 ">
                    <div class="table-responsive">
                        <table class="table card-table table-vcenter text-nowrap">
                            <thead class="text-white" style="background-color:#7bc144">
                                <tr>
                                    <th style="color:white;text-align:left;font-weight:bold;">Location</th>
                                    <th style="color:white;text-align:left;font-weight:bold;">Count</th>


                                    @*<th style="color:white;">Total</th>*@


                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var d in ViewBag.TrailerLocation as List<TrackerMVCEntities.BusinessEntities.TrailersEntities>)
                                {

                                    <tr>
                                        <td class="bg-primary text-white" style="text-align:left;padding: 10px 10px 0px 10px; cursor: pointer;">@d.Location </td>

                                        <td style="text-align:right;" onclick="ShowTrailerList(@d.LocationID,@d.Count)">@d.Count</td>



                                    </tr>
                                }

                            </tbody>
                        </table>
                    </div>

                </div>

            </div>


        </div>
        <div class="col-lg-6 col-sm-12">

            <div class="card text-center" style="border: 1px solid #9f78ff;" id="importArrivalMainCard">
                <div>
                    <div class="inline">
                        <div>
                            <img class="arrival_image" src="~/images/Location.png">
                            <!-- <i class="mdi mdi-truck-trailer left import_icon arrival"></i> -->
                        </div>
                        <div class="widget-line mt-5 margin_wrap">
                            <p>Navkar CFS</p>
                            <h1 class="counter">@ViewBag.NavkarLocationCount</h1>
                        </div>

                    </div>
                </div>
                <div class="mx-auto chart-circle chart-circle-md donutShadow" data-value="1.2951" data-thickness="20" data-color="#9f78ff">
                    <div class="chart-circle-value fs"><i class="fa fa-random"></i></div>
                </div>
                <div class="widget-line mt-5 ">
                    <div class="table-responsive">
                        <table class="table card-table table-vcenter text-nowrap">
                            <thead class="text-white" style="background-color:#7bc144">
                                <tr>
                                    <th style="color:white;text-align:left;font-weight:bold;">Location</th>
                                    <th style="color:white;text-align:left;font-weight:bold;">Count</th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var d in ViewBag.TrailerLocationICD as List<TrackerMVCEntities.BusinessEntities.TrailersEntities>)
                                {

                                    <tr>
                                        <td class="bg-primary text-white" style="text-align:left;padding: 10px 10px 0px 10px; cursor: pointer;">@d.Location </td>

                                        <td style="text-align:right;" onclick="ShowTrailerList(@d.LocationID,@d.Count)">@d.Count</td>



                                    </tr>
                                }

                            </tbody>
                        </table>
                    </div>

                </div>

            </div>


        </div>
        <!--bar graph-->

    </div>
</div>

