<script type="text/javascript">
    function MovementPendency() {
        $.ajax({
            type: 'POST',
            url: '/BL/MovementPendancy1',
            //data: '{FromDate: ' + JSON.stringify(FromDate) + ', ToDate: ' + JSON.stringify(ToDate) + ',DeliveryType: ' + JSON.stringify(DeliveryType) + ',For: ' + JSON.stringify(For) + ',PortID: ' + JSON.stringify(PortID) + ',LineID: ' + JSON.stringify(LineID) + '}',
            //data: JSON.stringify(formData),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: true,
            success: function (data) {
                $('#example1').dataTable({
                    "destroy": true,
                    "bLengthChange": false,
                    "aaData": data,
                    "paging": false,
                    "aaSorting": [[0, 'asc']],
                    "columns": [
                        { "data": "SrNo" },
                        { "data": "VesselName" },
                        { "data": "ViaNo" },
                        { "data": "ETA" },
                        { "data": "AGING" },
                        { "data": "TERMINAL" },
                        { "data": "BirthingDateandTime" },
                        { "data": "HBLNumber" },
                        { "data": "BLNumber" },
                        { "data": "BLReceivedDate" },
                        { "data": "BLEntrydate" },
                        { "data": "ShippingLine" },
                        { "data": "Size" },
                        { "data": "Teus" },
                        { "data": "ShipperName" },
                        { "data": "ImporterName" },
                        { "data": "CHAName" },
                        { "data": "BillingParty" },
                        { "data": "HaulageType" },
                        { "data": "Commodity" },
                        { "data": "IGMStatus" },
                        { "data": "ModeOfTransport" },
                        { "data": "PortOfLoading" },
                        { "data": "ContainerNo" },
                        { "data": "ScanType" },
                        { "data": "Remarks" },
                        { "data": "SMTPDate" },
                        { "data": "OutDateFromPort" },
                        { "data": "InTransit" },
                        { "data": "MarketingPerson" },
                        { "data": "KAM" },
                        { "data": "KAMTeamManagementTeamLastCheckedOn" }
                    ],
                    "dom": 'Bfrtip',
                    "buttons": [{
                        "extend": 'excel',
                        "title": 'Movement Pendency (BL)',
                        "filename": 'MovementPendencyBL'

                    }]
                });
                var table = document.getElementById("example1");
                var row;


                var TotTEUS1 = 0;
                var TotTEUS2 = 0;
                var TotTEUS3 = 0;
                var TotTEUS = 0;

                for (var i = 1; i < table.rows.length; i++) {
                    row = table.rows[i];
                    //var Size = row.cells[6].childNodes[1].value;
                    var Size = row.cells[12].innerText;

                    //alert(Size)

                    if (Size == 20) {
                        TotTEUS1 = Number(TotTEUS1) + 1;
                    }
                    if (Size == 40) {
                        TotTEUS2 = Number(TotTEUS2) + 1;
                    }
                    if (Size == 45) {
                        TotTEUS3 = Number(TotTEUS3) + 1;
                    }
                    //TotTEUS = Number(TotTEUS) + Number(size);


                }
                TotTEUS = TotTEUS1 + TotTEUS2 * 2 + TotTEUS3 * 2
                // alert(TotTEUS);
                $("#lbltwenty").text(TotTEUS1);
                $("#lblforty").text(TotTEUS2);
                $("#lblfortyfive").text(TotTEUS3);
                $("#lblTotal").text(TotTEUS);

                $("#divTEUS").show();
                $("#divTEUS1").show();
                $("#divTEUS2").show();
                $("#divTEUS3").show();
            },
            error: function (errormessage) {
                alert(errormessage.responseText);
            }
        });
    }
</script>

