<script>

    function Showdetails() {
        $("#Divsummary").show();
        $("#diventryscrren").hide();


    }

    function getBack() {
        $("#Divsummary").hide();
        $("#diventryscrren").show();
    }


    function FillDetails() {
     
        var Size = $("#ddlsize").val();
     
        if (Size == "20") {
            $("#ddlcontainertype").val(1);
            $("#dllIsocode").val(170);
            $("#TxttareWeight").val(2200);
        }
        if (Size == "40") {
            $("#ddlcontainertype").val(9);
            $("#dllIsocode").val(11);
            $("#TxttareWeight").val(3800);
        }
        if (Size == "45") {
            $("#ddlcontainertype").val(9);
            $("#dllIsocode").val(11);
            $("#TxttareWeight").val(3800);
        }
        if (Size == "") {
            $("#ddlcontainertype").val(0);
            $("#dllIsocode").val(0);
            $("#TxttareWeight").val(0);
        }
    }




