    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}



<script src="~/js/GetTableJsonPagging.js"></script>

<div id="tracker-loader"></div>

<style>
    .hiderow {
        display: none;
    }
</style>
<style>
    .table thead th, .text-wrap table thead th {
