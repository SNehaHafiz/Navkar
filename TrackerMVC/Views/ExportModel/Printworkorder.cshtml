

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    @*<title>FCl Custom Examine tally</title>*@
    <style>

        body {
            color: black;
        }

        * {
            box-sizing: border-box;
        }

        .clearfix::after {
            content: "";
            clear: both;
            display: table;
        }

        .box {
            float: left;
            width: 100%;
            padding: 0px;
        }

        .boxHalf {
            float: left;
            width: 50%;
            padding: 0px;
            border: 1px solid gray;
        }

        .boxHeaderL {
            /*float: left;*/
            width: 20%;
            padding: 0px;
            text-align: left;
        }

        .boxHeaderR {
            /*float: left;*/
            width: 80%;
            padding: 0px;
            text-align: center;
        }

        .tblBox {
            border-collapse: collapse;
            width: 100%;
            padding: 0px;
            float: left;
        }

        .headingh3 {
            font-size: 15pt;
            font-family: 'verdana(batlic)';
            padding: 2px;
            text-decoration: underline;
        }

        th {
            border: 1px solid black;
            font-size: 12px;
            font-family: 'Segoe UI';
        }


        td {
            border: 1px solid black;
            font-size: 12px;
            font-family: 'Segoe UI';
        }


        h5, h4 {
            font-size: 10pt;
            font-family: 'Segoe UI';
        }

        h3 {
            font-size: 10pt;
            font-family: 'verdana(batlic)';
        }

        .buttomMargin {
            margin-bottom: -20px;
            padding-bottom: 0px;
        }

        .footer {
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            background-color: red;
            color: white;
            text-align: center;
        }
    </style>
</head>
<body>
    <div style="width:100%; text-align:center; height:100%">
        <div style="width:100%;height:100%;">
            <table class="tblBox">
                <tbody>
                    <tr>
                      
                        <th style="text-align:left;width:7%"> <img height="70" width="200" src="~/images/NavkarLogo.jpg" /></th>
                        <th style="width:80%;text-align:center;background-color:white;font-family: 'Segoe UI'">
                            <spam style="width:80%;font-family: 'Segoe UI';font-weight:bold;font-size:20px">@ViewBag.con_Name</spam>   <br />
                            <spam style="font-family: 'Segoe UI';font-size:small;font-weight:400"> @ViewBag.AddressI</spam> <br />
                        </th>
                        <th style="width:10%;text-align:center;background-color:white"></th>

                    </tr>
                </tbody>
            </table>
            <table class="tblBox">
                <tbody>
                    <tr>
                        <th style="width:100%;text-align:center; background-color: #ede5e5;font-size:20px">
                            @ViewBag.Wo_Type
                        </th>
                    </tr>
                </tbody>
            </table>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <th style="width:15%;text-align:left;border:none;font-weight:400">Work Order No</th>
                                <td style="width:35%;text-align:left;border:none;font-weight:bold">:<label style="visibility:hidden">Sp</label>     @ViewBag.WO_NO</td>
                                <th style="width:15%;text-align:left;border:none;font-weight:400">Work Order Date</th>
                                <td style="width:35%;text-align:left;border:none;font-weight:bold">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.AddedOn</td>
                            </tr>
                            <tr>
                                <th style="width:15%;text-align:left;border:none;font-weight:400">IGM No</th>
                                <td style="width:35%;text-align:left;border:none;font-weight:bold">:<label style="visibility:hidden">Sp</label>     @ViewBag.IGMNo</td>
                                <th style="width:15%;text-align:left;border:none;font-weight:400">Item No</th>
                                <td style="width:35%;text-align:left;border:none;font-weight:bold">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.ItemNo</td>
                            </tr>
                            <tr>
                                <th style="width:15%;text-align:left;border:none;font-weight:400">CHA Name</th>
                                <td style="width:35%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:bold">Sp</label>     @ViewBag.CHAName</td>
                                <th style="width:15%;text-align:left;border:none;font-weight:400">Impoter Name</th>
                                <td style="width:35%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:bold">Sp</label>     @ViewBag.Consignee</td>
                            </tr>
                            @*<tr>
            <th style="width:15%;text-align:left;border:none;font-weight:400">Vendor Name</th>
            <td style="width:35%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:bold">Sp</label>     @ViewBag.VendorName</td>

        </tr>*@
                            <tr>
                                <th style="width:15%;text-align:left;border:none;font-weight:400">BL No</th>
                                <td style="width:35%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:bold">Sp</label>     @ViewBag.BLNO</td>

                                <th style="width:15%;text-align:left;border:none;font-weight:400">Line Name</th>
                                <td style="width:35%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:bold">Sp</label>     @ViewBag.SLName</td>

                            </tr>
                            <tr>

                                <th style="width:15%;text-align:left;border:none;font-weight:400">BOE No</th>
                                <td style="width:35%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:bold">Sp</label> @ViewBag.BOENo </td>

                                <th style="width:15%;text-align:left;border:none;font-weight:400">Do Validate</th>
                                <td style="width:35%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:bold">Sp</label>     @ViewBag.DoValidate</td>

                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div>
                &nbsp;
            </div>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox" id="tablegetdetails">
                        <thead>
                            <tr>
                                <th style="background-color: #ede5e5;width:5px">Sr No.</th>
                                <th style="background-color: #ede5e5;width:5px">Container No</th>
                                <th style="background-color: #ede5e5;width:5px">Size</th>
                                <th style="background-color: #ede5e5;width:5px">Seal No</th>
                                <th style="background-color: #ede5e5;width:5px">Vehicle No</th>
                                <th style="background-color: #ede5e5;width:5px">Arrival Date</th>
                                <th style="background-color: #ede5e5;width:5px">Total Cargo PKGS</th>
                                <th style="background-color: #ede5e5;width:5px">Total Cargo Weight</th>
                                <th style="background-color: #ede5e5;width:10px">Destuff PKGS</th>
                                <th style="background-color: #ede5e5;width:10px">Destuff Weight</th>
                                
                                <th style="background-color: #ede5e5;width:10px">Equipment Name</th>
                                <th style="background-color: #ede5e5;width:10px">Remarks</th>
                             
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var dr in ViewBag.ImportFCLdetailsCustomtally)
                            {
                                <tr>
                                    <td style="text-align:center;width:5px">@dr["Sr No"]</td>
                                    <td style="text-align:center;width:5px;font-weight:bold">@dr["ContainerNo"]</td>
                                    <td style="text-align:center;width:5px">@dr["Size"]</td>
                                    <td style="text-align:center;width:5px">@dr["IGM_SealNo"]</td>
                                    <td style="text-align:center;width:5px">@dr["vehicleno"]</td>
                                    <td style="text-align:center;width:5px">@dr["indate"]</td>
                                    <td style="text-align:center;width:5px">@dr["PKGSDestuff"]</td>
                                    <td style="text-align:center;width:5px">@dr["IGM_GrossWt"]</td>
                                    <td style="text-align:center;width:10px">@dr["IGM_Qty"]</td>
                                    <td style="text-align:center;width:10px">@dr["Weight"]</td>
      
                                    <td style="text-align:center;width:10px">@dr["EquipmentName"]</td>
                                    <td style="text-align:center;width:10px">@dr["remarks"]</td>
                           
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>

            <div>
                <br />
                ------------------------------------------------------------------------------------------------------------------------------------
                &nbsp;
            </div>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <td style="width:30%;text-align:left;border:none">Commodity Description :@ViewBag.IGM_GoodsDesc</td>
                            </tr>
                            <tr>
                                <td style="width:30%;text-align:left;border:none;visibility:hidden">Remarks :</td>
                            </tr>

                        </tbody>
                    </table>
                </div>
            </div>
            <div>
                <label style="visibility:hidden">Hello</label><br />
                <label style="visibility:hidden">Hello</label><br />
                <label style="visibility:hidden">Hello</label><br />
            </div>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <td style="width:18%;text-align:left;border:none;visibility:hidden">(CHA)</td>
                                <td style="width:23%;text-align:center;border:none;visibility:hidden">(CFS Executive)</td>
                                <td style="width:35%;text-align:center;border:none">
                                    @ViewBag.SurveyorName
                                </td>
                                <td style="width:24%;text-align:right;border:none;visibility:hidden">(Customs Officer)</td>

                            </tr>

                        </tbody>
                    </table>
                </div>
            </div>
            <div>
                <label style="visibility:hidden">Hello</label><br />
                <label style="visibility:hidden">Hello</label><br />
                <label style="visibility:hidden">Hello</label><br />
            </div>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <td style="width:18%;text-align:left;border:none">(Signature of CHA)</td>
                                <td style="width:23%;text-align:center;border:none">(Signature of CFS Executive)</td>
                                <td style="width:35%;text-align:center;border:none">
                                    @*(Surveyors and loss asseffors Pvt Ltd)*@
                                    (sign on import gate pass)
                                </td>
                                <td style="width:24%;text-align:right;border:none">(Signature of Customs Officer)</td>

                            </tr>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div>
        </div>
    </div>
</body>
</html>
