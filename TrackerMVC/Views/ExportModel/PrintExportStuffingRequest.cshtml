<html>
<head>
    <meta name="viewport" content="width=device-width" />


    <style>
        body {
            color: black;
        }

        * {
            box-sizing: border-box;
        }

        .clearfix::after {
            content: "";
            clear: both;
            display: table;
        }

        .box {
            float: left;
            width: 100%;
            padding: 0px;
        }

        .boxHalf {
            float: left;
            width: 50%;
            padding: 0px;
            border: 1px solid gray;
        }

        .boxHeaderL {
            /*float: left;*/
            width: 20%;
            padding: 0px;
            text-align: left;
        }

        .boxHeaderR {
            /*float: left;*/
            width: 80%;
            padding: 0px;
            text-align: center;
        }

        .tblBox {
            border-collapse: collapse;
            width: 100%;
            padding: 0px;
            float: left;
        }

        .headingh3 {
            font-size: 15pt;
            font-family: 'Segoe UI';
            padding: 2px;
            text-decoration: underline;
        }

        th {
            border: 1px solid black;
            font-size: 12px;
            font-family: 'Segoe UI';
        }


        td {
            border: 1px solid black;
            font-size: 12px;
            font-family: 'Segoe UI';
        }


        h5, h4 {
            font-size: 10pt;
            font-family: 'Segoe UI';
        }

        h3 {
            font-size: 10pt;
            font-family: 'Segoe UI';
        }

        .buttomMargin {
            margin-bottom: -20px;
            padding-bottom: 0px;
        }

        .footer {
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            background-color: red;
            color: white;
            text-align: center;
        }
    </style>
</head>
<body>
    <div style="width:100%; text-align:center; height:100%">

        <div style="width:100%;height:100%;">
            <table class="tblBox">
                <tbody>
                    <tr style="border:none">
                        <th style="text-align:left;width:10% !important"> <img src="~/images/NavkarLogo.jpeg" /></th>
                        <th style="width:80%;text-align:center;background-color:white;font-family: 'Segoe UI'">
                            <spam style="width:80%;font-family: 'Segoe UI';font-weight:bold;font-size:20px">@ViewBag.con_Name</spam>   <br />
                            <spam style="font-family: 'Segoe UI';font-size:small;font-weight:400"> @ViewBag.AddressI</spam> <br />

                        </th>
                        <th style="width:10%;text-align:center;background-color:white"></th>


                    </tr>

                </tbody>
            </table>
            <table class="tblBox">
                <tbody>
                    <tr>
                        <th style="width:100%;text-align:center; background-color: #ede5e5;font-size:20px">
                            Stuffing Request Form
                        </th>
                    </tr>

                </tbody>
            </table>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <th style="width:10%;text-align:left;border:none;font-weight:bold;padding-left:15px">To,</th>
                                <td style="width:90%;text-align:left;border:none;font-weight:400;visibility:hidden" colspan="3">:<label style="visibility:hidden">Sp</label> </td>
                            </tr>
                            <tr>
                                <th style="width:20%;text-align:left;border:none;font-weight:bold;padding-left:15px">@ViewBag.con_Name</th>
                                <td style="width:80%;text-align:left;border:none;font-weight:400;visibility:hidden" colspan="3">:<label style="visibility:hidden">Sp</label></td>
                            </tr>
                            <tr>
                                <th style="width:10%;text-align:left;border:none;font-weight:400;padding-left:15px">Dear sir/madam,</th>
                                <td style="width:90%;text-align:left;border:none;font-weight:400;visibility:hidden" colspan="3">:<label style="visibility:hidden">Sp</label></td>
                            </tr>

                            <tr>
                                <th style="width:90%;text-align:left;border:none;font-weight:400;padding-left:30px">Kindly permit stuffing of below containers with cargo of the following shippers under preventive supervision.</th>
                                <td style="width:10%;text-align:left;border:none;font-weight:400;visibility:hidden" colspan="3">:<label style="visibility:hidden">Sp</label></td>
                            </tr>

                        </tbody>
                    </table>
                </div>
            </div>

            <hr />

            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <th style="width:15%;text-align:left;border:none;font-weight:400;font-weight:bold">Request ID</th>
                                <td style="width:40%;text-align:left;border:none;">:<label style="visibility:hidden">Sp</label> @ViewBag.StuffRequestID</td>
                                <th style="width:10%;text-align:left;border:none;font-weight:400;font-weight:bold">Request Date</th>
                                <td style="width:30%;text-align:left;border:none;">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.RequestDate</td>
                            </tr>
                            <tr>
                                <th style="width:10%;text-align:left;border:none;font-weight:400;font-weight:bold">Shipper Name</th>
                                <td style="width:80%;text-align:left;border:none;font-weight:400" colspan="3">:<label style="visibility:hidden">Sp</label> @ViewBag.shippername</td>

                            </tr>
                            <tr>
                                <th style="width:10%;text-align:left;border:none;font-weight:400;font-weight:bold">Line Name</th>
                                <td style="width:80%;text-align:left;border:none;font-weight:400" colspan="3">:<label style="visibility:hidden">Sp</label> @ViewBag.SLName</td>

                            </tr>
                            <tr>
                                <th style="width:10%;text-align:left;border:none;font-weight:400;font-weight:bold">On account</th>
                                <td style="width:80%;text-align:left;border:none;font-weight:400" colspan="3">:<label style="visibility:hidden">Sp</label> @ViewBag.AGName</td>

                            </tr>
                            <tr>
                                <th style="width:15%;text-align:left;border:none;font-weight:400;font-weight:bold">Vessel</th>
                                <td style="width:40%;text-align:left;border:none;">:<label style="visibility:hidden;font-weight:bold">Sp</label> @ViewBag.VesselName</td>
                                <th style="width:10%;text-align:left;border:none;font-weight:400;font-weight:bold">Via No</th>
                                <td style="width:30%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.ViaNo</td>
                            </tr>
                            <tr>
                                <th style="width:15%;text-align:left;border:none;font-weight:400;font-weight:bold">FPD</th>
                                <td style="width:40%;text-align:left;border:none;">:<label style="visibility:hidden;font-weight:bold">Sp</label> @ViewBag.FPD</td>
                                <th style="width:10%;text-align:left;border:none;font-weight:400;font-weight:bold">Location</th>
                                <td style="width:30%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:bold">Sp</label> @ViewBag.Rotation</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div>
                &nbsp;
            </div>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox" id="tablegetdetails">
                        <thead>
                            <tr>
                                <th style="background-color: #ede5e5;width:5px">Sr No.</th>
                                <th style="background-color: #ede5e5;width:20px">SB No</th>
                                <th style="background-color: #ede5e5;width:10px">Invoice No</th>
                                <th style="background-color: #ede5e5;width:10px">Container No</th>
                                <th style="background-color: #ede5e5;width:10px">Size</th>
                                <th style="background-color: #ede5e5;width:10px">Cargo Description</th>
                                <th style="background-color: #ede5e5;width:10px">Sb Qty</th>
                                <th style="background-color: #ede5e5;width:10px">Qty.</th>


                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var dr in ViewBag.ImportloadedDetails)
                            {
                                <tr>
                                    <td style="text-align:center;width:5px">@dr["Sr No"]</td>
                                    <td style="text-align:center;width:20px;font-weight:bold">@dr["SBNo"]</td>
                                    <td style="text-align:center;width:10px">@dr["InvoiceNo"]</td>
                                    <td style="text-align:center;width:10px">@dr["ContainerNo"]</td>
                                    <td style="text-align:center;width:10px">@dr["CSize"]</td>
                                    <td style="text-align:center;width:10px">@dr["Cargodesc"]</td>
                                    <td style="text-align:center;width:10px">@dr["TotalPKGS"]</td>
                                    <td style="text-align:center;width:10px"></td>



                                </tr>
                            }
                        </tbody>

                    </table>
                </div>
            </div>
            <div>
                &nbsp;
            </div>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <td style="width:30%;text-align:center;border:none;visibility:hidden">Cha's Name and Sign </td>
                                <td style="width:30%;text-align:center;border:none;font-weight:bold">CUSTOM SUPERINTENDENT</td>
                                <td style="width:30%;text-align:center;border:none;font-weight:bold">MIV LOGISTICS PVT LTD</td>


                            </tr>
                          




                        </tbody>
                    </table>
                </div>
            </div>
            <div>
                &nbsp;
            </div>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <td style="width:30%;text-align:center;border:none;font-weight:bold">Cha's Name and Sign </td>
                                <td style="width:30%;text-align:center;border:none;font-weight:bold">Signature & stamp</td>
                                <td style="width:30%;text-align:center;border:none;font-weight:bold">Authorized Signatory</td>


                            </tr>
                            <tr>
                                <td style="width:30%;text-align:center;border:none">_______________________________________</td>
                                <td style="width:30%;text-align:center;border:none">_______________________________________</td>
                                <td style="width:30%;text-align:center;border:none">_______________________________________</td>


                            </tr>




                        </tbody>
                    </table>
                </div>
            </div>
            <div>
                &nbsp;
            </div>

            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <td style="width:15%;text-align:center;border:none;font-weight:bold">Prepared by :</td>
                                <td style="width:25%;text-align:center;border:none;font-weight:bold">@ViewBag.UserName</td>
                                <td style="width:30%;text-align:center;border:none">@ViewBag.PrintDate     &nbsp; @ViewBag.PrintTime</td>
                                <td style="width:30%;text-align:center;border:none;visibility:hidden">_______________________________________</td>


                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

        </div>
        <div>

        </div>
    </div>
</body>
</html>
