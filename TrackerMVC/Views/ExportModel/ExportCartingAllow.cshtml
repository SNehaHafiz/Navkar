                            <div class="form-group">
                                <label class="form-label">Allow Qty</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.TextBox("txtDelQty", null, new { @class = "form-control ", @id = "txtDelQty", autocomplete = "off", OnChange = "GetWeight()" })
                                    </div>

                                </div>
