                                            new SelectListItem { Text = "<1> SB <Many> Vehicle", Value = "<1> SB <Many> Vehicle"},
                                            new SelectListItem { Text = "<1> Vehicle <Many> SB", Value = "<1> Vehicle <Many> SB"},

                                            }, new { @class = "form-control", autocomplete = "off", @id = "ddlCondition" })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">SB No</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.TextBox("txtSBNo", null, new { @class = "form-control ", @id = "txtSBNo", autocomplete = "off", Placeholder = "NEW" })
                                    </div>

                                </div>
                            </div>
                        </div>
