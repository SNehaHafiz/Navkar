    $(window).on("load", function (e) {
        $("#ddlLocation").val(2);
        $("#ddlAllotedSpace").val('General Space');
    })

    function ViewReceiptSummary() {
        $("#divviewsummary").show();
        $("#DivReceiptGenDet").hide();
    }

    function getBack() {
        $("#divviewsummary").hide();
        $("#DivReceiptGenDet").show();
    }

    function Search() {
        
        $("#tracker-loader").fadeIn("slow");
        var Fromdate = $("#FromDate").val();
        var Todate = $("#ToDate").val();

        
        data1 = { 'Fromdate': Fromdate, 'Todate': Todate};
        data = JSON.stringify(data1);


        $.ajax({
            type: 'POST',
            url: '/ExportModel/SearchView',
            data: data,
            // data: "",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: true,
            success: function (jsonResult) {
                $("#exampleSearch").empty();
                RepTableDataJsonwopaging($("#exampleSearch"), jsonResult, "ExportCartingAllow", "ExportModel");
                $("#tracker-loader").fadeOut("slow");
            },
            error: function (errormessage) {
                alert(errormessage.responseText);
                $("#tracker-loader").fadeOut("slow");
            }
        });
    }

    function GetWeight() {
        
        var TotalQty = $("#txtQty").val();
        var TotalWeight = $("#txtWeight").val();
        var AllowQty = $("#txtDelQty").val();
        if (TotalWeight == "") {
            TotalWeight = 0;
        }


        if (parseFloat(TotalQty) < parseFloat(AllowQty)) {
            alert("allow Qty is not greater then total Qty!");
           
        }


        DelWeight = ((parseFloat(TotalWeight) / parseFloat(TotalQty)) * AllowQty);
        $("#txtDelWeight").val(DelWeight);


    }
    function OnAddSBNo() {
        
        var ddlCondition = document.getElementById('ddlCondition').value;

        if (ddlCondition == 0) {
            document.getElementById("ddlCondition").style.borderColor = "red";
            alert('Condition cannot be left blank ')
            $("#tracker-loader").fadeOut("slow");
            event.preventDefault();
            return;
        }

        var SBNo = $("#txtSBNo").val();
        var data1 = { SBNo: SBNo };
        var data = JSON.stringify(data1);
        $.ajax({
            url: '/ExportModel/OnAddSBNo',
            type: 'Post',
            data: data,
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                
                $("#txtAgencyName").val(data[0].agency);
                $("#txtInvoiceno").val(data[0].invoice);
                if (data[0].invoice == "") {
                    $("#txtInvoiceno").val(SBNo);
                }
                $("#lblsbentryid").val(data[0].sbentryid);
                $("#txtChaCode").val(data[0].cha);
                $("#txtExporter").val(data[0].exporter);
                $("#txtCargoDescription").val(data[0].cargodesc);
                $("#txtQty ").val(data[0].qty);
                $("#txtWeight").val(data[0].wt);

                $("#txtCargoType ").val(data[0].cargotype);
                $("#txtStuffingType").val(data[0].stuffingtype);
                $("#txtBalanceQty").val(data[0].bqty);
                $("#txtBalanceQty1").val(data[0].bqty1);
            },
            error: function (errormessage) {
                alert(errormessage.responseText);
                $("#tracker-loader").fadeOut("slow");
            }
        });
        
    }

    var storeAddedItem = [];
    function Add() {
        
        InvoiceNo = $("#txtInvoiceno").val();
        Vehicleno = $("#txtVehicleNo").val();

        var TotalQty = $("#txtQty").val();
        var AllowQty = $("#txtDelQty").val();
        if (InvoiceNo == "") {
            document.getElementById("txtInvoiceno").style.borderColor = "red";
            alert('Please add Invoice No!');
            return;
        }

        if (Vehicleno == "") {
            document.getElementById("txtVehicleNo").style.borderColor = "red";
            alert('Please add the vehicle no!');
            return;
        }
        if (AllowQty == "") {
            document.getElementById("txtDelQty").style.borderColor = "red";
            alert('Please add Allow Qty!');
            return;
        }


        if (parseFloat(TotalQty) < parseFloat(AllowQty)) {
            alert("allow Qty is not greater then total Qty!");
            return false;
        }



        var element = {};
        element.VehicleNo = $('#txtVehicleNo').val();
        element.LocationName = $("#ddlLocation option:selected").text();
        element.Area = $('#txtArea').val();
        element.Space = $("#ddlAllotedSpace option:selected").text();
        element.DelQty = $('#txtDelQty').val();
        element.ALLOWwT = $('#txtDelWeight').val();
        element.SBNo = $('#txtSBNo').val();
        element.SBEntryID = $('#lblsbentryid').val();
        element.InvoiceNo = $('#txtInvoiceno').val();
        element.Unit = $('#txtUnitQty').val();
       
        
        storeAddedItem.push(element);


        $('#example').DataTable({
            "destroy": true,
            "bLengthChange": false,
            "aaData": storeAddedItem,
            "bPaginate": false,
            "bInfo": false,
            "paging": false,
            "columns": [
                { "data": "VehicleNo" },
                { "data": "LocationName" },
                { "data": "Area" },
                { "data": "Space" },
                { "data": "DelQty" },
                { "data": "ALLOWwT" },
                { "data": "SBNo" },
                { "data": "SBEntryID" },
                { "data": "InvoiceNo" },
                { "data": "Unit" }
            ]
        });
        var ddlCondition = $("#ddlCondition option:selected").text();
        if (ddlCondition == "<1> SB <Many> Vehicle") {
            alert('record successfully added! more vehicle to be added?')
            document.getElementById('txtVehicleNo').value = "";
            document.getElementById('txtDelQty').value = "";
            document.getElementById('txtDelWeight').value = "";
            document.getElementById('txtArea').value = "";
        }
        if (ddlCondition == "<1> Vehicle <Many> SB") {
            alert('record successfully added! more Shipping Bill to be added?')
            document.getElementById('txtSBNo').value = "";
            document.getElementById('lblsbentryid').value = "";
        }
    }

    function Save() {
         
         ///ddlentryType
        //CheckVelid()
        var CartingAllowNo = "0"
        var AllowFromDate = $("#txtAllowFromDate").val();
            var SelectedData = new Array();
            var tablearray = new Array();
            var table = document.getElementById("example");
            var row;
            for (var i = 1; i < table.rows.length; i++) {
                row = table.rows[i];
                VehicleNo = row.cells[0].innerText;

                LocationName = row.cells[1].innerText;
                Area = row.cells[2].innerText;
                Space = row.cells[3].innerText;
                DelQty = row.cells[4].innerText;
                ALLOWwT = row.cells[5].innerText;
                SBNo = row.cells[6].innerText;
                SBEntryID = row.cells[7].innerText;
                InvoiceNo = row.cells[8].innerText;
                Unit = row.cells[9].innerText;
                SelectedData.push({
                    'VehicleNo': VehicleNo, 'LocationName': LocationName, 'Area': Area, 'Space': Space, 'DelQty': DelQty, 'ALLOWwT': ALLOWwT, 
                    'SBNo': SBNo, 'SBEntryID': SBEntryID, 'InvoiceNo': InvoiceNo, 'Unit': Unit
                })
            }
            var data1 = {
                'Debitdata': SelectedData, 'CartingAllowNo': CartingAllowNo, 'AllowFromDate': AllowFromDate
            };
            var data = JSON.stringify(data1);
                          $.ajax({
                              url: "/ExportModel/Save",
                            data: data,
                            type: "POST",
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                              success: function (data) {
                                  alert(data);
                                  document.location = '@Url.Action("exportcartingallow", "ExportModel")';
                                  
                            },
                            error: function (errormessage) {
                                alert(errormessage.responseText);
                            }
                        });
 
         
    }


    function CheckVelid() {
        
       
        var SBNo = $("#txtSBNo").val();
        var sbentryid = $("#lblsbentryid").val();
        var data1 = { 'SBNo': SBNo, 'sbentryid': sbentryid};
        var data = JSON.stringify(data1);
        $.ajax({
            url: '/ExportModel/WithReason',
            type: 'Post',
            data: data,
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                alert(data);
                return false;
            },
            error: function (errormessage) {
                alert(errormessage.responseText);
                $("#tracker-loader").fadeOut("slow");
            }
        });

    }


    function PrintCartingallow(CartingAllowID) {

        var data1 = { 'CartingAllowID': CartingAllowID };
        var data = JSON.stringify(data1);
        $.ajax({
            url: "/ExportModel/PrintCartingAllow",
            data: data,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            // dataType: "json",
            success: function (response) {

                var myWindow = window.open("", "MsgWindow1");
                myWindow.close();
                var myWindow = window.open("", "MsgWindow1");
                myWindow.document.write(response);
            },

            error: function (errormessage) {
                alert(errormessage.responseText);
            }
        });
    }
</script>