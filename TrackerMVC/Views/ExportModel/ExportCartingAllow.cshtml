                        </div>

                        <div class="col-lg-1 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">Unit Qty</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.TextBox("txtUnitQty", null, new { @class = "form-control ", @id = "txtUnitQty", autocomplete = "off", Placeholder = "" })
                                    </div>

                                </div>
                            </div>
                        </div>

                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">Allow Weight</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.TextBox("txtDelWeight", null, new { @class = "form-control ", @id = "txtDelWeight", autocomplete = "off", Placeholder = "" })
                                    </div>

                                </div>
                            </div>
                        </div>

                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">Area</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.TextBox("txtArea", null, new { @class = "form-control ", @id = "txtArea", autocomplete = "off", Placeholder = "" })
                                    </div>

                                </div>
                            </div>
                        </div>
                        
                    </div>
                    <div class="row">
                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">Location</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.DropDownList("ddlLocation", (IEnumerable<SelectListItem>)ViewBag.ActivityL, "--Select--", new { @class = "form-control", @id = "ddlLocation", name = "ddlLocation", autocomplete = "off" })

                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">Alloted Space</label>
                                <div class="row gutters-xs">
                                    <div class="col">
                                        @Html.DropDownList("ddlAllotedSpace", new List<SelectListItem>
                                          {  new SelectListItem { Text = "--Select--", Value = "0"},
                                              new SelectListItem { Text = "General Space", Value = "General Space"},
                                            new SelectListItem { Text = "Reserved Space", Value = "Reserved Space"},

                                            }, new { @class = "form-control", autocomplete = "off", @id = "ddlAllotedSpace" })
                                    </div>
                                </div>
