<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <style>

        body {
            color: black;
        }

        * {
            box-sizing: border-box;
        }

        .clearfix::after {
            content: "";
            clear: both;
            display: table;
        }

        .box {
            float: left;
            width: 100%;
            padding: 0px;
        }

        .boxHalf {
            float: left;
            width: 50%;
            padding: 0px;
            border: 1px solid gray;
        }

        .boxHeaderL {
            /*float: left;*/
            width: 20%;
            padding: 0px;
            text-align: left;
        }

        .boxHeaderR {
            /*float: left;*/
            width: 80%;
            padding: 0px;
            text-align: center;
        }

        .tblBox {
            border-collapse: collapse;
            width: 100%;
            padding: 0px;
            float: left;
        }

        .headingh3 {
            font-size: 15pt;
            font-family: 'Segoe UI';
            padding: 2px;
            text-decoration: underline;
        }

        th {
            border: 1px solid black;
            font-size: 10px;
            font-family: 'Segoe UI';
        }


        td {
            border: 1px solid black;
            font-size: 10px;
            font-family: 'Segoe UI';
        }


        h5, h4 {
            font-size: 10pt;
            font-family: 'Segoe UI';
        }

        h3 {
            font-size: 10pt;
            font-family: 'Segoe UI';
        }

        .buttomMargin {
            margin-bottom: -20px;
            padding-bottom: 0px;
        }

        .footer {
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            background-color: red;
            color: white;
            text-align: center;
        }
    </style>
</head>
<body>
    <div style="width:100%; text-align:center; height:100%">
        <div style="width:100%;height:100%;">
            <hr style="background-color: #ede5e5;" />
            <table class="tblBox">
                <tbody>
                    <tr>

                        <th style="text-align:left;width:10%;border:none"><img height="70" width="200" src="~/images/NavkarLogo.jpg" /></th>
                        <th style="width:80%;text-align:center;background-color:white;font-family: 'Segoe UI';border:none">
                            <spam style="width:80%;font-family: 'Segoe UI';font-weight:bold;font-size:20px">@ViewBag.con_Name</spam>   <br />
                            <spam style="font-family: 'Segoe UI';font-size:small;font-weight:400"> @ViewBag.AddressI</spam> <br />
                            <spam style="font-family: 'Segoe UI';font-size:small;font-weight:400"> @ViewBag.GSTINNo</spam> <br />
                        </th>
                        <th style="text-align:left;width:10%;border:none"> <spam style="font-family: 'Segoe UI';font-size:small;font-weight:400;visibility:hidden"> @ViewBag.GSTINNo</spam></th>
                    </tr>
                </tbody>
            </table>
            <table class="tblBox" style="border:none !important">
                <tbody>
                    <tr>
                        <th style="width:100%;text-align:center; background-color: #ede5e5;font-size:15px;border:none">
                            Back To Town -Gate Pass
                        </th>
                    </tr>
                </tbody>
            </table>
            <span style="font-size:1px;visibility:hidden">.</span>
            <hr style="border: 0.5px solid;background-color: #ede5e5;" />
            <hr style="background-color: #ede5e5;" />

            <spam style="font-family: 'Calibri (Body)';font-size:small;font-weight:400;text-align:center"></spam>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <th style="width:5%;text-align:left;border:none;font-weight:bold">Gate Pass No</th>
                                <td style="width:45%;text-align:left;border:none;">:<label style="visibility:hidden">Sp</label>     @ViewBag.GPNO</td>
                                <th style="width:5%;text-align:left;border:none;font-weight:bold">SB Date</th>
                                <td style="width:25%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.GPDate</td>
                            </tr>
                            <tr>
                                <th style="width:15%;text-align:left;border:none;font-weight:bold">Agency Name</th>
                                <td style="width:45%;text-align:left;border:none">:<label style="visibility:hidden">Sp</label>     @ViewBag.AGName</td>
                                @*<th style="width:15%;text-align:left;border:none;font-weight:bold">Vehicle No</th>
                                    <td style="width:45%;text-align:left;border:none">:<label style="visibility:hidden">Sp</label>     @ViewBag.trailerno</td>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">Vessel</th>
                                    <td style="width:25%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.Vessel</td>*@
                            </tr>
                            @*<tr>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">Transporter</th>
                                    <td style="width:45%;text-align:left;border:none">:<label style="visibility:hidden">Sp</label>     @ViewBag.TransName</td>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">Via No</th>
                                    <td style="width:25%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.gpviano</td>

                                </tr>*@
                            @*<tr>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">A\C Holder</th>
                                    <td style="width:45%;text-align:left;border:none">:<label style="visibility:hidden">Sp</label>     @ViewBag.AGName</td>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">POD</th>
                                    <td style="width:25%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.POD</td>

                                </tr>*@
                            @*<tr>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">Exporter</th>
                                    <td style="width:45%;text-align:left;border:none">:<label style="visibility:hidden">Sp</label>     @ViewBag.shippername</td>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">POL</th>
                                    <td style="width:25%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.Port</td>

                                </tr>*@
                            @*<tr>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">CHA Name</th>
                                    <td style="width:45%;text-align:left;border:none">:<label style="visibility:hidden">Sp</label>     @ViewBag.CHAName</td>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">SB No</th>
                                    <td style="width:25%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.SBNumber</td>

                                </tr>*@
                            @*<tr>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">Driver</th>
                                    <td style="width:45%;text-align:left;border:none">:<label style="visibility:hidden">Sp</label> @ViewBag.drivername </td>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">Transport Type </th>
                                    <td style="width:25%;text-align:left;border:none">:<label style="visibility:hidden;font-weight:400">Sp</label> @ViewBag.Transport_Type</td>

                                </tr>
                                <tr>
                                    <th style="width:15%;text-align:left;border:none;font-weight:bold">Train No</th>
                                    <td style="width:45%;text-align:left;border:none">:<label style="visibility:hidden">Sp</label>     @ViewBag.TrainNo</td>

                                </tr>*@
                        </tbody>
                    </table>
                </div>
            </div>
            <div>
                &nbsp;
            </div>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox" id="tablegetdetails">
                        <thead>
                            <tr>
                                <th style="background-color: #ede5e5;width:10px">SB No</th>
                                <th style="background-color: #ede5e5;width:10px">SB Date    </th>
                                <th style="background-color: #ede5e5;width:10px">BTT Qty</th>
                                <th style="background-color: #ede5e5;width:10px">BTT Wt Type</th>
                                <th style="background-color: #ede5e5;width:10px">Vehicle No</th>
                                <th style="background-color: #ede5e5;width:10px">CHA</th>
                                <th style="background-color: #ede5e5;width:10px">Exporter</th>
                                <th style="background-color: #ede5e5;width:10px">Cargo Description</th>
                                @* <th style="background-color: #ede5e5;width:10px">Gross Weight</th>
        <th style="background-color: #ede5e5;width:10px">Shipping Line</th>*@

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var dr in ViewBag.ImportloadedDetails)
                            {
                            <tr>
                                <td style="text-align:center;width:10px;font-weight:bold;font-size:18px">@dr["SBNO"]</td>
                                <td style="text-align:center;width:10px;font-size:18px">@dr["SBDate"]</td>
                                <td style="text-align:center;width:10px;font-size:18px">@dr["BttQty"]</td>
                                <td style="text-align:center;width:10px;font-size:18px">@dr["bttwt"]</td>
                                <td style="text-align:center;width:10px;font-size:18px">@dr["TrailerNo"]</td>
                                <td style="text-align:center;width:10px;font-size:18px">@dr["CHAName"]</td>
                                <td style="text-align:center;width:10px;font-size:18px">@dr["shippername"]</td>
                                <td style="text-align:center;width:10px;font-size:18px">@dr["CargoDesc"]</td>
                                @*<td style="text-align:center;width:10px;font-size:18px">@dr["CSealNo"]</td>
        <td style="text-align:center;width:10px;font-size:18px">@dr["pkgs"]</td>
        <td style="text-align:center;width:10px;font-size:18px">@dr["NetWeight"]</td>
        <td style="text-align:center;width:10px;font-size:18px">@dr["GrossWeight"]</td>
        <td style="text-align:center;width:10px;font-size:18px">@dr["SLName"]</td>*@

                            </tr>}
                        </tbody>
                    </table>
                </div>
            </div>
            <div>
                &nbsp;
            </div>
            <div>
                <label style="visibility:hidden">Hello</label><br />
                <label style="visibility:hidden">Hello</label><br />
                <label style="visibility:hidden">Hello</label><br />
            </div>
            <div class="printBody clearfix">
                <div class="box">
                    <table class="tblBox">
                        <tbody>
                            <tr>
                                <td style="width:30%;text-align:center;border:none">_______________________________________</td>
                                <td style="width:30%;text-align:center;border:none">_______________________________________</td>
                                <td style="width:30%;text-align:center;border:none">_______________________________________</td>

                            </tr>
                            <tr>
                                <td style="width:30%;text-align:center;border:none">Cha's Name and Sign </td>
                                <td style="width:30%;text-align:center;border:none">CFS Executive Name & Sign</td>
                                <td style="width:30%;text-align:center;border:none">(Signature of Customs Officer)</td>

                            </tr>

                        </tbody>
                    </table>
                </div>
            </div>
            <div>
                <label style="visibility:hidden">Hello</label><br />
                <label style="visibility:hidden">Hello</label><br />
                <label style="visibility:hidden">Hello</label><br />
            </div>
            <div style="text-align:left">
                <table class="tblBox">
                    <tbody>
                        <tr>
                            <th style="width:10%;text-align:left;border:none;font-weight:bold">Prepared by</th>
                            <td style="width:20%;text-align:left;border:none;">:<label style="visibility:hidden">Sp</label>     @ViewBag.UserName</td>

                        </tr>
                    </tbody>
                </table>
            </div>
            <hr style="background-color: #ede5e5;" />
            <div style="text-align:left">
                <table class="tblBox">
                    <tbody>
                        <tr>
                            <th style="width:15%;text-align:left;border:none;font-weight:bold">Print Date & Time</th>
                            <td style="width:85%;text-align:left;border:none;">:<label style="visibility:hidden">Sp</label>     @ViewBag.PrintDate</td>

                        </tr>

                    </tbody>
                </table>
            </div>
        </div>
        <div>
        </div>
    </div>
</body>
