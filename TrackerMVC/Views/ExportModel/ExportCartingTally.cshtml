    function RcvdQty() {

        var Weight = $("#txtWeight").val();
        var RcvdQty = $("#txtRcvdQty").val();
        var QTy = $("#txtQty").val();

        RcvdWt = parseFloat(Weight) * parseFloat(RcvdQty) / parseFloat(QTy)
        $("#txtRcvdWt").val(RcvdWt);


