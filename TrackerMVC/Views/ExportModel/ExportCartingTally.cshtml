    }

    function OpenTariffDetails() {

        $("#largeModal1").modal('show')
        GetDetailsForTariffSearch
    }
    function GetDetailsForTariffSearch() {
        $("#tracker-loader").fadeOut("slow");
        ddlSearchType
        var SearchType = $("#ddlSearchType").val();
        var Search = $("#TXtSearchOn").val();

        var data1 = { 'SearchType': SearchType, 'Search': Search };
        var data = JSON.stringify(data1);
        $.ajax({
            type: "POST",
            url: "/ExportModel/GetTariffDetailsForSearch",
            data: data,
            contentType: "application/json; charset=utf-8",
            success: function (data) {

                $("#example3").empty();
                RepTableDataJsonwopaging($("#example3"), data, "Voucher Details", "VoucherDetails_List");
                $("#tracker-loader").fadeOut("slow");
            },
            error: function (response) {
                alert(response.responseText);
                $("#tracker-loader").fadeOut("slow");
            }
        })
    }

    function GetCartingDetails1() {
      var  CartingID = $("#TxtCrting").val();
        var data1 = { 'SBNo': CartingID };
        var data = JSON.stringify(data1);
        $.ajax({
            url: "/ExportModel/ExportCartingSbNo",
            type: 'Post',
            data: data,
            async: false,
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                $("#TxtCrting").val(data[0].AllowID);
                $("#txtAgencyName").val(data[0].AgencyName);

                $('#example').dataTable({
                    "destroy": true,
                    "bLengthChange": false,
                    "bPaginate": false,
                    //"bFilter": false,
                    "paging": false,
                    "order": false,
                    "aaData": data,
                    "columns": [
                        { "data": "SRNO" },
                        { "data": "SBNo" },
                        { "data": "VehicleNo" },
                        { "data": "GateInQty" },
                        { "data": "GateInWt" },
                        {

                            "data": "RcvdQty",
                            "render": function (data, type, row, meta) {
                                data = '<input type=\"textbox\" name=\"textReceivedQTY' + row.SRNO + '\" class=\"form-control\" id=\"textReceivedQTY' + row.SRNO + '\"   style=\"width:120px;text-transform:uppercase\"    value="' + data + '">';
                                return data;
                            }
                        },
                        {

                            "data": "RcvdWt",
                            "render": function (data, type, row, meta) {
                                data = '<input type=\"textbox\" name=\"TxtReceivedWt' + row.SRNO + '\" class=\"form-control\" id=\"TxtReceivedWt' + row.SRNO + '\"   style=\"width:120px;text-transform:uppercase\"    value="' + data + '">';
                                return data;
                            }
                        },
                        {

                            "data": "RcvdWt",
                            "render": function (data, type, row, meta) {
                                data = '<input type=\"textbox\" name=\"TxtArea' + row.SRNO + '\" class=\"form-control\" id=\"TxtArea' + row.SRNO + '\"   style=\"width:120px;text-transform:uppercase\"    value="' + data + '">';
                                return data;
                            }
                        },
                        {
                            "data": "Equipment",
                            "render": function (data, type, row, meta) {

                                data = '<select  class="form-control" id="Equipment' + row.SRNO + '"  style="width:100px">'
                                for (j = 0; j < EquipmentL.length; j++) {

                                    data += '<option value="' + EquipmentL[j].EnID + '"> ' + EquipmentL[j].EquipmentType + '</option>'

                                }

                                data += '</select>';
                                return data;

                            }

                        },

                        {
                            "data": "Loaction",
                            "render": function (data, type, row, meta) {

                                data = '<select  class="form-control" id="Location' + row.SRNO + '"  style="width:100px">'
                                for (j = 0; j < LocationL.length; j++) {

                                    data += '<option value="' + LocationL[j].LocationIID + '"> ' + LocationL[j].Location + '</option>'

                                }
                                data += '</select>';
                                return data;
                            }

                        },
                        {
                            "data": "AllotedSpace",
                            "render": function (data, type, row, meta) {

                                data = '<select  class="form-control" id="Allocated' + row.SRNO + '"  style="width:100px">'
                                for (j = 0; j < AllocatedL.length; j++) {

                                    data += '<option value="' + AllocatedL[j].AllocatedID + '"> ' + AllocatedL[j].AllocatedName + '</option>'

                                }

                                data += '</select>';
                                return data;
                            }
                        },
                       
                        { "data": "SBentryid" },
                        { "data": "AllowID" },
                        
                        { "data": "Unit" },

                    ]
                });



            },
            error: function (response) {
                alert(response.responseText);
            }
        })
    }


