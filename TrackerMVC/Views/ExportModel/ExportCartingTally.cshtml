                                              {  new SelectListItem { Text = "All", Value = "All"},
                                                  new SelectListItem { Text = "CHA", Value = "CHA"},
                                                new SelectListItem { Text = "Agency", Value = "Agency"},
                                                new SelectListItem { Text = "Location", Value = "Location"},
                                                new SelectListItem { Text = "Shipper", Value = "Shipper"},
                                                }, new { @class = "form-control", autocomplete = "off", @id = "ddlCategory" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-1 col-sm-12" style="display:none" id="divCHA">
