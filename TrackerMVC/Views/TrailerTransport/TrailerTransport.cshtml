                                        <option value="0">--Select--</option>
                                        <option value="True">True</option>
                                        <option value="false">false</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-12">
                        <div class="form-group">
                            <label class="form-label mandatory">Vehicle group type</label>
                            <div class="row gutters-xs">
                                <div class="col">
                                    @Html.DropDownListFor(model => model.Vehicletypegroup, (IEnumerable<SelectListItem>)ViewBag.VehicleGroupType, "--Select--", new { @class = "form-control", @id = "ddlVehicletypegroup", name = "ddlVehicletypegroup", autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-12">
                        <div class="form-group">
                            <label class="form-label mandatory">Fuel Type</label>
                            <div class="row gutters-xs">
                                <div class="col">
                                    @Html.DropDownListFor(model => model.Fueltype, (IEnumerable<SelectListItem>)ViewBag.Fueltype, "--Select--", new { @class = "form-control", @id = "ddlFuelType", name = "ddlFuelType", autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">

                </div>
                <div class="row">
