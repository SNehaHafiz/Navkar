                                        @Html.TextBox("txtAddpermitdate", null, new { @class = "form-control form_datetime1", @id = "txtAddpermitdate", name = "txtAddpermitdate", autocomplete = "on" })


                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">

                    </div>

                </div>
                <div class="row">
                    <div class="col-lg-3 col-sm-12">
                        <div class="form-group">
                            <label class="form-label mandatory">Vehicle Group</label>
                            <div class="row gutters-xs">
                                <div class="col">
                                    <select class="form-control" id="ddlvehiclegroup">
                                        <option value="0">--Select--</option>
                                        <option value="NCL">NCL</option>
                                        <option value="NTL">NTL</option>
                                        <option value="TP">TP</option>
                                        <option value="SOLD">SOLD</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-12">
