<script>
    function EditTrailerDetails(trailerid, trailername, OwnedBy, TransName, Passing, Grade, ModelNo, ChasisNo, EngineNo, trailerType, Permit, PermitExpiry, TransID, CheckIsActive, VehicleGroup, Isshiftingint, VehicleGrouptype, VehicleGrouptypeID,
        Fitness_Due_Date, Tax_Due_Date, Policy_End_Date, Green_Tax_Due, Puc_date,
        Usedfor, VehicleTypeidEdit, TrolleySizecount, Groupid, Fuelid, Model, SoldDate, MFGYR, Mileage, GVW) {

      

        $("#divheader").hide();
        $("#diventrytable").hide();
        $("#divtrailerMaasterView").hide();
           $("#divedit").show();
       
        document.getElementById('txttrailerid').value = trailerid;
        document.getElementById('txttrailernumber').value = trailername;
        document.getElementById('ddlOwnedBy1').value = OwnedBy;
        document.getElementById('ddltransporter1').value = TransID;
        document.getElementById('txtpassing').value = Passing;
        document.getElementById('txtgrade').value = Grade;
        document.getElementById('txtmodelno').value = ModelNo;
        document.getElementById('txtchasisno').value = ChasisNo;
        document.getElementById('txtengineno').value = EngineNo;
        document.getElementById('ddltrailertype').value = trailerType;
        document.getElementById('txtpermit').value = Permit;
        document.getElementById('txtpermitdate').value = PermitExpiry;
        document.getElementById('ddlEditvehiclegroup').value = VehicleGroup;
        document.getElementById('TxtFitnessDate').value = Fitness_Due_Date;
        document.getElementById('TxttaxDueDate').value = Tax_Due_Date;
        document.getElementById('TxtPolicyDate').value = Policy_End_Date;
        document.getElementById('TxtGreenTax').value = Green_Tax_Due;
        document.getElementById('TxtPucDate').value = Puc_date;
        document.getElementById('ddlusedfor').value = Usedfor;
        document.getElementById('ddlVehicleGrouptypeEdit').value = Groupid;
        document.getElementById('ddlFueltypeEdit').value = Fuelid;
        $("#ddlModel1 option:selected").text(Model);
        //document.getElementById('ddlModel1').value = Model;
        document.getElementById('txtSoldDateEdit').value = SoldDate;


        document.getElementById('txtMFGYREdit').value = MFGYR;
        document.getElementById('txtMileageEDIt').value = Mileage;
        document.getElementById('txtGVWEDIT').value = GVW;
      
        document.getElementById('ddlEditVehicleType').value = VehicleTypeidEdit;
           if (CheckIsActive == 0) {
               document.getElementById("chkIsActiveEdit").checked = false;
           } else {
               document.getElementById("chkIsActiveEdit").checked = true;
        }
        document.getElementById('ddlEditisshifting').value = Isshiftingint;
       
        document.getElementById('ddlEditvehicleGroupType').value = VehicleGrouptypeID;
      
        document.getElementById('ddltrailerSize').value = TrolleySizecount;
       



     }



    function Edittrailer() {
        debugger;
         var res = validateEditFunction();
        if (res == false) {
           return false;
         }
         var trailerid = $("#txttrailerid").val();
         var trailerNo = $("#txttrailernumber").val();
         var ownedby = $("#ddlOwnedBy1").val();
         var Transportername = $("#ddltransporter1").val();
         var passing = $("#txtpassing").val();
         var grade = $("#txtgrade").val();
         var modelno = $("#txtmodelno").val();
         var chasisno = $("#txtchasisno").val();
         var engine = $("#txtengineno").val();
         var trailertype = $("#ddltrailertype").val();
         var permit = $("#txtpermit").val();
         var permittype = $("#txtpermitdate").val();
      
         var EditchkIsActive = document.getElementById("chkIsActiveEdit").checked;
         var EditVehicleTYpe = $("#ddlEditvehiclegroup").val();
         var EditIsshifting = $("#ddlEditisshifting").val();
         var EditVehicleGroupType = $("#ddlEditvehicleGroupType").val();
         var PolicyDate = $("#TxtPolicyDate").val();
         var GreenTax = $("#TxtGreenTax").val();
         var PucDate = $("#TxtPucDate").val();
         var UsedFor = $("#ddlusedfor").val();
         var Taxdate = $("#TxttaxDueDate").val();
         var Fitnessdate = $("#TxtFitnessDate").val();
         var EditVEhicleTypeID = $("#ddlEditVehicleType").val();
         var EditVendorName = $("#EditddlVendorName option:selected").text();
         var TrailerSize = $("#ddltrailerSize option:selected").val();
         var VehicleGroupType = $("#ddlVehicleGrouptypeEdit option:selected").val();
         var FuelType = $("#ddlFueltypeEdit option:selected").val();
        var Model = $("#ddlModel1").val();
        var SoldDateEdit = $("#txtSoldDateEdit").val();
        var MFGYREdit = $("#txtMFGYREdit").val();
        var MileageEDIt = $("#txtMileageEDIt").val();
        var GVWEDIT = $("#txtGVWEDIT").val();
       

         var data1 = {
             'trailerid': trailerid, 'trailerNo': trailerNo, 'ownedby': ownedby, 'Transportername': Transportername,
             'passing': passing, 'grade': grade, 'modelno': modelno, 'chasisno': chasisno, 'engine': engine,
             'trailertype': trailertype, 'permit': permit, 'permittype': permittype, 'EditchkIsActive': EditchkIsActive,
             'EditVehicleTYpe': EditVehicleTYpe, 'EditIsshifting': EditIsshifting, 'EditVehicleGroupType': EditVehicleGroupType,
             'PolicyDate': PolicyDate, 'GreenTax': GreenTax, 'PucDate': PucDate, 'UsedFor': UsedFor, 'Taxdate': Taxdate,
             'Fitnessdate': Fitnessdate, 'EditVEhicleTypeID': EditVEhicleTypeID, 'EditVendorName': EditVendorName, 'TrailerSize': TrailerSize,
             'VehicleGroupType': VehicleGroupType, 'FuelType': FuelType, 'Model': Model, 'SoldDateEdit': SoldDateEdit, 'MFGYREdit': MFGYREdit,
             'MileageEDIt': MileageEDIt, 'GVWEDIT': GVWEDIT
         }
         var data = JSON.stringify(data1);
        
         $.ajax({
             url: "/TrailerTransport/AjaxUpdateTrailerDetails",
             data: data,
             type: "POST",
             contentType: "application/json; charset=utf-8",
             dataType: "json",
             success: function (data) {
                 alert(data);
                 window.location.href = '@Url.Action("TrailerTransport", "TrailerTransport")';
             },
             error: function (errormessage) {
                 alert(errormessage.responseText);
             }
         });

     }
</script>

