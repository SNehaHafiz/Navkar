<script>
    function validateEditFunction() {

        var edittrailerno = document.getElementById('txttrailernumber').value;
        var edirownedby = document.getElementById('ddlOwnedBy1').value;
        var ddltransporter = document.getElementById('ddltransporter1').value;
        var valtxtpassing = document.getElementById('txtpassing').value;
        var valtxtengime = document.getElementById('txtengineno').value;
        var valtrailertype = document.getElementById('ddltrailertype').value;
        var valpermit = document.getElementById('txtpermit').value;
        var valpermitdate = document.getElementById('txtpermitdate').value;
        var VehicleModelType = document.getElementById('ddlEditvehicleGroupType').value;


        var blResult = Boolean;
        blResult = true;

        document.getElementById("txttrailernumber").style.borderColor = "gainsboro";
        document.getElementById("ddlOwnedBy1").style.borderColor = "gainsboro";
        document.getElementById("ddltransporter1").style.borderColor = "gainsboro";
        document.getElementById("txtpassing").style.borderColor = "gainsboro";
        document.getElementById("txtengineno").style.borderColor = "gainsboro";
        document.getElementById("ddltrailertype").style.borderColor = "gainsboro";
        document.getElementById("txtpermit").style.borderColor = "gainsboro";
        document.getElementById("txtpermitdate").style.borderColor = "gainsboro";
        document.getElementById("ddlEditvehicleGroupType").style.borderColor = "gainsboro";


        var transportercheck = $("#ddltransporter1 option:selected").val();


        if (transportercheck != 37) {

            if (edittrailerno == "") {
                //  document.getElementById('ContainerNo').style.borderColor = "red"
                document.getElementById("txttrailernumber").style.borderColor = "red";
                blResult = blResult && false;
            }



            if (edirownedby == "") {
                document.getElementById("ddlOwnedBy1").style.borderColor = "red";

                blResult = blResult && false;
            }
            if (ddltransporter == "") {
                document.getElementById("ddltransporter1").style.borderColor = "red";
                blResult = blResult && false;
            }
            if (valtxtpassing == "") {
                document.getElementById("txtpassing").style.borderColor = "red";
                blResult = blResult && false;
            }

            if (valtxtengime == "") {
                document.getElementById("txtengineno").style.borderColor = "red";
                blResult = blResult && false;
            }
            if (valtrailertype == "") {
                document.getElementById("ddltrailertype").style.borderColor = "red";
                blResult = blResult && false;
            }
            if (valpermit == 0) {
                document.getElementById("txtpermit").style.borderColor = "red";
                blResult = blResult && false;
            }
            if (valpermitdate == "") {
                document.getElementById("txtpermitdate").style.borderColor = "red";
                blResult = blResult && false;
            }
            if (VehicleModelType == 0) {
                document.getElementById("ddlEditvehicleGroupType").style.borderColor = "red";
                blResult = blResult && false;
            }
        }



        if (blResult == false) { alert('Please fill the required fields!'); }
        return blResult;

    };
</script>