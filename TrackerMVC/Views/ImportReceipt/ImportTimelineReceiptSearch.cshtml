

<style>



    ul.timeline {
        list-style-type: none;
        position: relative;
    }

        ul.timeline:before {
            content: ' ';
            background: #d4d9df;
            display: inline-block;
            position: absolute;
            left: 29px;
            width: 2px;
            height: 100%;
            z-index: 400;
        }

        ul.timeline > li {
            margin: 20px 0;
            padding-left: 20px;
        }

            ul.timeline > li:before {
                content: ' ';
                background: #fff !important;
                display: inline-block;
                position: absolute;
                border-radius: 50%;
                border: 3px solid #22c0e8;
                left: 20px;
                width: 20px;
                height: 20px;
                z-index: 400;
            }
</style>
<style>

    table#example3 th {
        background-color: #88d317;
        text-align: center;
        color: white;
    }



    table#example3 td:first-child {
        /*border-color: orangered;*/
        background-color: #9f78ff;
        color: black;
    }
</style>


<script src="~/js/GetTableJsonwopaging.js"></script>
<ul class="timeline">
    @if (ViewBag.TimeLine != null)
    {
        foreach (var d in ViewBag.TimeLine)
        {
            <li>
                <a class="pl-6" href="#" id="aDivShow" data-toggle="modal" title="View More" data-target="#exampleModal"><b>@d["Process"]</b></a>

                <br />

                <p class="pl-8">@d["Trans Date"] | <b> @d["Prepared By"] </b> </p>

            </li>
        }
    }


</ul>

<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" style="padding-top:100px;" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">

            <div class="modal-header" style="text-align:left;font-size:large;color:blue;font-weight: 700;">
                <b style="margin-top:4px;">Timeline:</b><input readonly="readonly" style="font-weight: bold;background-color:white;border-color:white" class="form-control" id="TxtCGSTs" />
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                <div class="col-md-12 col-lg-12">
                    <div class="table-responsive">
                        <table class="table table-bordered text-wrap" id="example3" style="font-weight: bold; width:100%">
                            <thead>

                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>



