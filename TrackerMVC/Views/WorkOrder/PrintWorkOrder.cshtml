@model IEnumerable<TrackerMVCEntities.BusinessEntities.WorkOrderEntities>

@{
    ViewBag.Title = "Work Order Print";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}

@{
    Layout = null;

}
<style>
    #page {
        size: landscape;
    }

    #first {
        float: left;
        width: 40%;
        height: 50%;
    }

    #gg {
        float: left;
        width: 40%;
        height: 50%;
    }

    #second {
        float: left;
        width: 60%;
        height: 30%;
    }

    .first1 {
        float: left;
        width: 50%;
        height: 100%;
    }

    .signature {
        border: 0;
        border-bottom: 1px solid #000;
    }

    table {
        border-collapse: collapse;
    }

    table, th, td {
        border: 1px solid black;
    }
</style>

<style type="text/css">
    table {
        orientation: landscape;
    }
</style>
<style>
    body {
        /*page-break-before: avoid;
        width: 100%;
        height: 100%;
        -webkit-transform: rotate(-270deg) ;
        -moz-transform: rotate(-270deg) ;*/
        size: landscape;
    }

    .backcolor {
        background-color: yellow;
    }
</style>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div style="align-content:center">

        <label style="margin-left:150px;font-family:'Segoe UI'">
            <b>  @ViewBag.CompanyName</b>
        </label><br />
        <label style="margin-left:100px;font-family:'Segoe UI'">
            <b> @ViewBag.Companyaddress</b><br />

        </label><br /><br>
        <label style="margin-left:150px;font-family:'Segoe UI'"><b>Export Work Order</b></label><br />

    </div><br />
    <div class="row">
        <label style="font-family:'Segoe UI'">
            Work Order No.:<b> @ViewBag.WONo</b>

        </label><br />
        <label style="font-family:'Segoe UI'">Date: &nbsp;&nbsp;@ViewBag.WODate</label>

    </div>
</body>
</html>