                                    <th style="background-color: #ede5e5;width:5px">Material Qty</th>
                                    <th style="background-color: #ede5e5;width:5px">Material Descriptions</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var dr in ViewBag.ImportFCLdetailsCustomtally)
                                {
                                <tr>
                                    <td style="text-align:center;width:5px">@dr["Sr No"]</td>
                                    <td style="text-align:center;width:5px">@dr["invoiceno"]</td>
                                    <td style="text-align:center;width:5px">@dr["vehicleno"]</td>
                                    <td style="text-align:center;width:5px">@dr["PKGSDestuff"]</td>
                                    <td style="text-align:center;width:5px">@dr["Weight"]</td>
                                    <td style="text-align:center;width:5px">@dr["PKGSRestuff"]</td>
                                    <td style="text-align:center;width:5px">@dr["CargoWeight"]</td>
